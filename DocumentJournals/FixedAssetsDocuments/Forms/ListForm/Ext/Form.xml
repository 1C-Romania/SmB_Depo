	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentJournalFixedAssetsDocuments.Ref,
	DocumentJournalFixedAssetsDocuments.Date,
	DocumentJournalFixedAssetsDocuments.DeletionMark,
	DocumentJournalFixedAssetsDocuments.Number,
	DocumentJournalFixedAssetsDocuments.Posted,
	DocumentJournalFixedAssetsDocuments.Company,
	DocumentJournalFixedAssetsDocuments.Counterparty,
	DocumentJournalFixedAssetsDocuments.Contract,
	DocumentJournalFixedAssetsDocuments.Comment,
	DocumentJournalFixedAssetsDocuments.Type,
	DocumentJournalFixedAssetsDocuments.Author
FROM
	DocumentJournal.FixedAssetsDocuments AS DocumentJournalFixedAssetsDocuments
WHERE
	(&amp;FixedAsset = VALUE(Catalog.FixedAssets.EmptyRef)
			OR DocumentJournalFixedAssetsDocuments.Ref In
				(SELECT
					FixedAssetsOutput.Ref
				FROM
					Document.FixedAssetsOutput.FixedAssets AS FixedAssetsOutput
				WHERE
					FixedAssetsOutput.FixedAsset = &amp;FixedAsset
		
				UNION ALL
		
				SELECT
					ChangingParametersFAFixedAssets.Ref
				FROM
					Document.FixedAssetsModernization.FixedAssets AS ChangingParametersFAFixedAssets
				WHERE
					ChangingParametersFAFixedAssets.FixedAsset = &amp;FixedAsset
		
				UNION ALL
		
				SELECT
					FixedAssetsTransferFX.Ref
				FROM
					Document.FixedAssetsTransfer.FixedAssets AS FixedAssetsTransferFX
				WHERE
					FixedAssetsTransferFX.FixedAsset = &amp;FixedAsset
		
				UNION ALL
		
				SELECT
					FixedAssetsEnterFixedAssets.Ref
				FROM
					Document.FixedAssetsEnter.FixedAssets AS FixedAssetsEnterFixedAssets
				WHERE
					FixedAssetsEnterFixedAssets.FixedAsset = &amp;FixedAsset
		
				UNION ALL
		
				SELECT
					FixedAssetsWriteOffOfFixedAssets.Ref
				FROM
					Document.FixedAssetsWriteOff.FixedAssets AS FixedAssetsWriteOffOfFixedAssets
				WHERE
					FixedAssetsWriteOffOfFixedAssets.FixedAsset = &amp;FixedAsset))</QueryText>
				<MainTable>DocumentJournal.FixedAssetsDocuments</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
