	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.DataIsCurrentPictureIndex</Field>
				<Field>List.DataIsCurrentFlag</Field>
				<Field>List.Code</Field>
				<Field>List.Advanced</Field>
				<Field>List.AddressObjectCodeInCode</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	InformationRegisterAddressClassifier.AddressItemType,
	InformationRegisterAddressClassifier.AddressObjectCodeInCode,
	InformationRegisterAddressClassifier.CountyCodeInCode,
	InformationRegisterAddressClassifier.CityCodeInCode,
	InformationRegisterAddressClassifier.SettlementCodeInCode,
	InformationRegisterAddressClassifier.StreetCodeInCode,
	InformationRegisterAddressClassifier.Code,
	InformationRegisterAddressClassifier.Description,
	InformationRegisterAddressClassifier.Abbr,
	InformationRegisterAddressClassifier.PostalCode,
	InformationRegisterAddressClassifier.AlternateNames,
	InformationRegisterAddressClassifier.DataIsCurrentFlag,
	
	CASE 
		WHEN InformationRegisterAddressClassifier.PostalCode LIKE "%[^ ]%" 
			// Index is filled
			THEN InformationRegisterAddressClassifier.PostalCode
		ELSE ""
	END
	+ 
	CASE 
		WHEN InformationRegisterAddressClassifier.AlternateNames LIKE "%[^ ]%" THEN
			// Alternate names are available
			CASE
				WHEN InformationRegisterAddressClassifier.PostalCode LIKE "%[^ ]%" THEN ", "
				ELSE ""
			END
			+ InformationRegisterAddressClassifier.AlternateNames
		ELSE
			""
	END AS Advanced,
	
	CASE 
		WHEN InformationRegisterAddressClassifier.DataIsCurrentFlag &gt; 0
		THEN 0
		ELSE 1
	END AS DataIsCurrentPictureIndex
	FROM
		InformationRegister.AddressClassifier AS InformationRegisterAddressClassifier</QueryText>
