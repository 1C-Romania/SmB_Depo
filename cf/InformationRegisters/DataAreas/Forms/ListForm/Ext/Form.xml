	InformationRegister.DataAreas AS InformationRegisterDataAreas
		LEFT JOIN Constant.DataAreaPresentation AS DataAreaPresentation
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreaPresentation.DataAreaAuxiliaryData
