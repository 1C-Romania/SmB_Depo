	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.ThisCheckingTableRights</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT DISTINCT
	AccessValuesSets.Object,
	AccessValuesSets.NumberOfSet,
	AccessValuesSets.AccessValue,
	CASE
		WHEN VALUETYPE(AccessValuesSets.AccessValue) = Type(Catalog.MetadataObjectIDs)
			THEN TRUE
		ELSE FALSE
	END AS ThisCheckingTableRights,
	AccessValuesSets.Read,
	AccessValuesSets.Update,
	AccessValuesSets.Adjustment,
	AccessValuesSets.StandardValue,
	AccessValuesSets.ValueWithoutGroups
	FROM
		InformationRegister.AccessValuesSets AS AccessValuesSets</QueryText>
				<MainTable>InformationRegister.AccessValuesSets</MainTable>
