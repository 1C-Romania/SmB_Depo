				<Field>List.AdditionalOrderingAttribute</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CASE
		WHEN ContactInformationKindsCatalog.IsFolder
			THEN CASE
					WHEN ContactInformationKindsCatalog.DeletionMark
						THEN 1
					WHEN ContactInformationKindsCatalog.Predefined
						THEN 2
					ELSE 0
				END
		WHEN ContactInformationKindsCatalog.DeletionMark
			THEN 4
		WHEN ContactInformationKindsCatalog.Predefined
			THEN CASE ContactInformationKindsCatalog.Type
					WHEN VALUE(Enum.ContactInformationTypes.Phone)
						THEN 14
					WHEN VALUE(Enum.ContactInformationTypes.EmailAddress)
						THEN 15
					WHEN VALUE(Enum.ContactInformationTypes.WebPage)
						THEN 16
					WHEN VALUE(Enum.ContactInformationTypes.Fax)
						THEN 17
					WHEN VALUE(Enum.ContactInformationTypes.Other)
						THEN 18
					WHEN VALUE(Enum.ContactInformationTypes.Address)
						THEN 19
					WHEN VALUE(Enum.ContactInformationTypes.Skype)
						THEN 21
					ELSE 3
				END
		ELSE CASE ContactInformationKindsCatalog.Type
				WHEN VALUE(Enum.ContactInformationTypes.Phone)
					THEN 7
				WHEN VALUE(Enum.ContactInformationTypes.EmailAddress)
					THEN 8
				WHEN VALUE(Enum.ContactInformationTypes.WebPage)
					THEN 9
				WHEN VALUE(Enum.ContactInformationTypes.Fax)
					THEN 10
				WHEN VALUE(Enum.ContactInformationTypes.Other)
					THEN 11
				WHEN VALUE(Enum.ContactInformationTypes.Address)
					THEN 12
				WHEN VALUE(Enum.ContactInformationTypes.Skype)
					THEN 20
				ELSE 3
			END
	END AS IconIndex,
	ContactInformationKindsCatalog.Ref,
	ContactInformationKindsCatalog.DeletionMark,
	ContactInformationKindsCatalog.Predefined,
	ContactInformationKindsCatalog.Parent,
	ContactInformationKindsCatalog.IsFolder,
	ContactInformationKindsCatalog.Description,
	ContactInformationKindsCatalog.Type,
	ContactInformationKindsCatalog.AdditionalOrderingAttribute,
	ContactInformationKindsCatalog.EditInDialogOnly,
	ContactInformationKindsCatalog.CanChangeEditMode,
	ContactInformationKindsCatalog.DomesticAddressOnly
FROM
	Catalog.ContactInformationKinds AS ContactInformationKindsCatalog</QueryText>
