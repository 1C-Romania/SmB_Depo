		<Autofill>false</Autofill>
		<ChildItems>
			<Button name="FormGoToList" id="3">
				<Type>CommandBarButton</Type>
				<DefaultButton>true</DefaultButton>
				<CommandName>Form.Command.GoToList</CommandName>
				<ExtendedTooltip name="FormGoToListExtendedTooltip" id="4"/>
			</Button>
			<Button name="FormChecked" id="1">
				<Type>CommandBarButton</Type>
				<CommandName>Form.Command.Checked</CommandName>
				<ExtendedTooltip name="FormCheckedExtendedTooltip" id="2"/>
			</Button>
			<Button name="FormClose" id="5">
				<Type>CommandBarButton</Type>
				<CommandName>Form.Command.Close</CommandName>
				<ExtendedTooltip name="FormCloseExtendedTooltip" id="6"/>
			</Button>
		</ChildItems>
