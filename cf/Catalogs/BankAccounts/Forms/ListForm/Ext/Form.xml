			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogBankAccounts.Ref,
	CatalogBankAccounts.DeletionMark,
	CatalogBankAccounts.Owner,
	CatalogBankAccounts.Code,
	CatalogBankAccounts.Description,
	CatalogBankAccounts.CashCurrency,
	CatalogBankAccounts.AccountNo,
	CatalogBankAccounts.Bank,
	CatalogBankAccounts.AccountsBank,
	CatalogBankAccounts.GLAccount,
	CatalogBankAccounts.AccountType,
	CatalogBankAccounts.CorrespondentText,
	CatalogBankAccounts.DestinationText,
	CatalogBankAccounts.MonthOutputOption,
	CatalogBankAccounts.AmountWithoutCents,
	CatalogBankAccounts.OpeningDate,
	CatalogBankAccounts.ClosingDate,
	CatalogBankAccounts.Predefined,
	CatalogBankAccounts.PredefinedDataName,
	CASE
		WHEN &amp;OwnerMainAccount = UNDEFINED
			THEN CatalogBankAccounts.Ref = CatalogBankAccounts.Owner.BankAccountByDefault
		ELSE CatalogBankAccounts.Ref = &amp;OwnerMainAccount
	END AS IsMainAccount
FROM
	Catalog.BankAccounts AS CatalogBankAccounts</QueryText>
