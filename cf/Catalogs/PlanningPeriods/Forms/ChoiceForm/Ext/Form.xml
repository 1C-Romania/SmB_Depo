			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogPlanningPeriods.Ref,
	CatalogPlanningPeriods.DeletionMark,
	CatalogPlanningPeriods.Predefined,
	CatalogPlanningPeriods.Code,
	CatalogPlanningPeriods.Description,
	CatalogPlanningPeriods.StartDate,
	CatalogPlanningPeriods.EndDate,
	CatalogPlanningPeriods.Periodicity
FROM
	Catalog.PlanningPeriods AS CatalogPlanningPeriods
WHERE
	CASE
			WHEN &amp;ExcludeActualPeriod
				THEN CatalogPlanningPeriods.Ref &lt;&gt; VALUE(Catalog.PlanningPeriods.Actual)
			ELSE TRUE
		END</QueryText>
