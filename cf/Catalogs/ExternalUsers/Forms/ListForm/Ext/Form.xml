		<Attribute name="ExternalUsersList" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>External users list</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Внешние пользователи список</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>ExternalUsersList.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	ExternalUsers.Ref,
	ExternalUsers.Prepared,
	ExternalUsers.DeletionMark,
	ExternalUsers.NotValid,
	ExternalUsers.Predefined,
	ExternalUsers.Description,
	ExternalUsers.Comment,
	CASE
		WHEN ExternalUsers.DeletionMark
			THEN 6
		ELSE 7
	END AS PictureNumber
	FROM
	Catalog.ExternalUsers AS ExternalUsers
	WHERE
	(&amp;AnyAuthorizationObjectType
			OR &amp;TypeOfAuthorizationObjects = VALUETYPE(ExternalUsers.AuthorizationObject))
	AND (&amp;SelectHierarchy
				AND TRUE In
					(SELECT
						TRUE
					FROM
						InformationRegister.UsersGroupsContents AS UsersGroupsContents
					WHERE
						UsersGroupsContents.User = ExternalUsers.Ref
						AND UsersGroupsContents.UsersGroup = &amp;ExternalUserGroup)
			OR Not &amp;SelectHierarchy
				AND TRUE In
					(SELECT
						TRUE
					FROM
						Catalog.ExternalUsersGroups.Content AS ExternalUsersGroupsContents
					WHERE
						ExternalUsersGroupsContents.ExternalUser = ExternalUsers.Ref
							AND ExternalUsersGroupsContents.Ref = &amp;ExternalUserGroup))</QueryText>
				<MainTable>Catalog.ExternalUsers</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:use>false</dcsset:use>
							<dcsset:left xsi:type="dcscor:Field">Prepared</dcsset:left>
							<dcsset:comparisonType>Equal</dcsset:comparisonType>
							<dcsset:right xsi:type="xs:boolean">true</dcsset:right>
							<dcsset:presentation xsi:type="xs:string">Prepared by the person responsible for the list</dcsset:presentation>
							<dcsset:viewMode>Normal</dcsset:viewMode>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>e391d2e4-4713-4515-aa35-61e44224b313</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>Description</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
							<dcsset:viewMode>Normal</dcsset:viewMode>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
