	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.UseScheduledJob</Field>
				<Field>List.UseImportFlag</Field>
				<Field>List.UseExportFlag</Field>
				<Field>List.UsageFlag</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DataExchangeScripts.Ref,
	DataExchangeScripts.UseScheduledJob,
	DataExchangeScripts.Description,
	ISNULL(NestedSelect.UsageFlag, FALSE) AS UsageFlag,
	ISNULL(NestedSelect.UseImportFlag, FALSE) AS UseImportFlag,
	ISNULL(NestedSelect.UseExportFlag, FALSE) AS UseExportFlag,
	DataExchangeScripts.Comment
FROM
	Catalog.DataExchangeScripts AS DataExchangeScripts
		LEFT JOIN (SELECT
			DataExchangeScenarioExchangeSettings.Ref AS Ref,
			DataExchangeScenarioExchangeSettings.Ref.UseScheduledJob AS UseScheduledJob,
			DataExchangeScenarioExchangeSettings.Ref.Description AS Description,
			MAX(CASE
					WHEN DataExchangeScenarioExchangeSettings.InfobaseNode = &amp;InfobaseNode
						THEN TRUE
					ELSE FALSE
				END) AS UsageFlag,
			MAX(CASE
					WHEN DataExchangeScenarioExchangeSettings.InfobaseNode = &amp;InfobaseNode
							AND DataExchangeScenarioExchangeSettings.RunningAction = VALUE(Enum.ActionsAtExchange.DataImport)
						THEN TRUE
					ELSE FALSE
				END) AS UseImportFlag,
			MAX(CASE
					WHEN DataExchangeScenarioExchangeSettings.InfobaseNode = &amp;InfobaseNode
							AND DataExchangeScenarioExchangeSettings.RunningAction = VALUE(Enum.ActionsAtExchange.DataExport)
						THEN TRUE
					ELSE FALSE
				END) AS UseExportFlag
		FROM
			Catalog.DataExchangeScripts.ExchangeSettings AS DataExchangeScenarioExchangeSettings
		WHERE
			Not DataExchangeScenarioExchangeSettings.Ref.DeletionMark
		
		GROUP BY
			DataExchangeScenarioExchangeSettings.Ref,
			DataExchangeScenarioExchangeSettings.Ref.UseScheduledJob,
			DataExchangeScenarioExchangeSettings.Ref.Description) AS NestedSelect
		ON (NestedSelect.Ref = DataExchangeScripts.Ref)
WHERE
	Not DataExchangeScripts.DeletionMark
</QueryText>
				<MainTable>Catalog.DataExchangeScripts</MainTable>
