	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
				<Field>List.Owner</Field>
				<Field>1/51</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogContactPersons.Ref AS Ref,
	CatalogContactPersons.DeletionMark AS DeletionMark,
	CatalogContactPersons.Predefined AS Predefined,
	CatalogContactPersons.Owner AS Owner,
	CatalogContactPersons.Code AS Code,
	CatalogContactPersons.Description AS Description,
	CatalogContactPersons.Ind AS Ind,
	CatalogContactPersons.ConnectionRegistrationDate AS ConnectionRegistrationDate,
	CatalogContactPersons.ConnectionStopDate AS ConnectionStopDate,
	CatalogContactPersons.Responsible AS Responsible,
	MAX(ContactPersonRolesOfContactPerson.ContactPersonRole) AS ContactPersonRole,
	CAST(CatalogContactPersons.Comment AS String(1000)) AS Comment
FROM
	Catalog.ContactPersons AS CatalogContactPersons
		LEFT JOIN Catalog.ContactPersons.ContactPersonRoles AS ContactPersonRolesOfContactPerson
		ON (ContactPersonRolesOfContactPerson.Ref = CatalogContactPersons.Ref)

GROUP BY
	CatalogContactPersons.Ref,
	CatalogContactPersons.DeletionMark,
	CatalogContactPersons.Predefined,
	CatalogContactPersons.Owner,
	CatalogContactPersons.Code,
	CatalogContactPersons.Description,
	CatalogContactPersons.Ind,
	CatalogContactPersons.ConnectionRegistrationDate,
	CatalogContactPersons.ConnectionStopDate,
	CatalogContactPersons.Responsible,
	CAST(CatalogContactPersons.Comment AS String(1000))</QueryText>
				<MainTable>Catalog.ContactPersons</MainTable>
