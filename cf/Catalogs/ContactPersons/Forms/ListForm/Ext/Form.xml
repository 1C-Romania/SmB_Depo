	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
				<Field>List.Owner</Field>
				<Field>List.Invalid</Field>
				<Field>List.AdditionalOrderingAttribute</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogContactPersons.Ref,
	CatalogContactPersons.DeletionMark,
	CatalogContactPersons.Owner,
	CatalogContactPersons.Code,
	CatalogContactPersons.Description,
	CatalogContactPersons.Position,
	CatalogContactPersons.Gender,
	CatalogContactPersons.DateOfBirth,
	CatalogContactPersons.Responsible,
	CatalogContactPersons.Invalid,
	CatalogContactPersons.CreationDate,
	CatalogContactPersons.RolesList,
	CatalogContactPersons.AdditionalOrderingAttribute,
	CAST(CatalogContactPersons.Comment AS STRING(1000)) AS Comment,
	ВЫБОР
		КОГДА &amp;MainCounterpartyContactPerson = НЕОПРЕДЕЛЕНО
			ТОГДА CatalogContactPersons.Ref = CatalogContactPersons.Owner.ContactPerson
		ИНАЧЕ CatalogContactPersons.Ref = &amp;MainCounterpartyContactPerson
	КОНЕЦ КАК IsMainContactPerson,
	CatalogContactPersons.Roles.(
		Ref,
		LineNumber,
		Role
	),
	CatalogContactPersons.ContactInformation.(
		Ref,
		LineNumber,
		Type,
		Kind,
		Presentation,
		FieldValues,
		Country,
		Region,
		City,
		EmailAddress,
		ServerDomainName,
		PhoneNumber,
		PhoneNumberWithoutCodes
	),
	CatalogContactPersons.AdditionalAttributes.(
		Ref,
		LineNumber,
		Property,
		Value,
		TextString
	)
FROM
	Catalog.ContactPersons AS CatalogContactPersons</QueryText>
