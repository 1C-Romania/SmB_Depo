<?xml version="1.0" encoding="UTF-8"?>
<Form xmlns="http://v8.1c.ru/8.3/xcf/logform" xmlns:app="http://v8.1c.ru/8.2/managed-application/core" xmlns:cfg="http://v8.1c.ru/8.1/data/enterprise/current-config" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:ent="http://v8.1c.ru/8.1/data/enterprise" xmlns:lf="http://v8.1c.ru/8.2/managed-application/logform" xmlns:style="http://v8.1c.ru/8.1/data/ui/style" xmlns:sys="http://v8.1c.ru/8.1/data/ui/fonts/system" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:web="http://v8.1c.ru/8.1/data/ui/colors/web" xmlns:win="http://v8.1c.ru/8.1/data/ui/colors/windows" xmlns:xr="http://v8.1c.ru/8.3/xcf/readable" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4">
	<WindowOpeningMode>LockOwnerWindow</WindowOpeningMode>
	<UseForFoldersAndItems>Items</UseForFoldersAndItems>
	<AutoCommandBar name="FormCommandBar" id="-1"/>
	<Events>
		<Event name="BeforeWrite">BeforeWrite</Event>
	</Events>
	<ChildItems>
		<InputField name="Description" id="4">
			<DataPath>Object.Description</DataPath>
			<Title>
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Шаблон</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Template</v8:content>
				</v8:item>
			</Title>
			<EditMode>EnterOnInput</EditMode>
			<Width>52</Width>
			<ContextMenu name="DescriptionContextMenu" id="5"/>
			<ExtendedTooltip name="DescriptionExtendedToolTip" id="6"/>
		</InputField>
		<InputField name="Owner" id="7">
			<EditMode>EnterOnInput</EditMode>
			<ContextMenu name="OwnerContextMenu" id="8"/>
			<ExtendedTooltip name="OwnerExtendedToolTip" id="9"/>
		</InputField>
		<LabelDecoration name="DecorationToolTip" id="10">
			<AutoMaxWidth>false</AutoMaxWidth>
			<Title formatted="true">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>&lt;i&gt;Наберите символы шаблона серийного номера в том порядке, в каком они будут вводиться&lt;/&gt;
&lt;i&gt;• Укажите # вместо цифр серийного номера, которые будут меняться&lt;/&gt;
&lt;i&gt;• Укажите префикс, суффикс или другие символы серийного номера в порядке их следования&lt;/&gt;
&lt;i&gt;Например: Для номера АА-000001 шаблон будет выглядеть так:АА-######&lt;/&gt;
</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>&lt;i&gt;Type the chars of the serial number template in the order in
which they going to be inputed&lt;/&gt; &lt;i&gt;• Specify # instead of numbers of the
serial number that supposed to be change&lt;/&gt; &lt;i&gt;• Specify a prefix, suffix or other chars of the serial number
sequentially&lt;/&gt; For example: For AA -000001 template will look like this: AA-######&lt;/&gt;
</v8:content>
				</v8:item>
			</Title>
			<BackColor>style:ToolTipBackColor</BackColor>
			<ContextMenu name="DecorationToolTipContextMenu" id="11"/>
			<ExtendedTooltip name="DecorationToolTipExtendedToolTip" id="12"/>
		</LabelDecoration>
	</ChildItems>
	<Attributes>
		<Attribute name="Object" id="1">
			<Type>
				<v8:Type>cfg:CatalogObject.SerialNumbersTemplates</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<SavedData>true</SavedData>
		</Attribute>
	</Attributes>
</Form>