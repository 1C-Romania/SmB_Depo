	<Attributes>
		<Attribute name="UsersList" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Users list</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Пользователи список</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>UsersList.Ref</Field>
				<Field>UsersList.NotValid</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Users.Ref,
	Users.Prepared,
	Users.DeletionMark,
	Users.NotValid,
	Users.Description,
	Users.Service,
	Users.Comment,
	CASE
		WHEN Users.DeletionMark
			THEN 0
		ELSE 1
	END AS PictureNumber
FROM
	Catalog.Users AS Users
WHERE
	(&amp;SelectHierarchy
				AND TRUE In
					(SELECT
						TRUE
					FROM
						InformationRegister.UsersGroupsContents AS UsersGroupsContents
					WHERE
						UsersGroupsContents.User = Users.Ref
						AND UsersGroupsContents.UsersGroup = &amp;UsersGroup)
			OR Not &amp;SelectHierarchy
				AND TRUE In
					(SELECT
						TRUE
					FROM
						Catalog.UsersGroups.Content AS UsersGroupsContents
					WHERE
						UsersGroupsContents.User = Users.Ref
							AND UsersGroupsContents.Ref = &amp;UsersGroup))</QueryText>
				<MainTable>Catalog.Users</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:use>false</dcsset:use>
							<dcsset:left xsi:type="dcscor:Field">Prepared</dcsset:left>
							<dcsset:comparisonType>Equal</dcsset:comparisonType>
							<dcsset:right xsi:type="xs:boolean">true</dcsset:right>
							<dcsset:presentation xsi:type="xs:string">Prepared by the person responsible for the list</dcsset:presentation>
							<dcsset:viewMode>Normal</dcsset:viewMode>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>73ec9c65-83f1-4821-af16-5dcbfa3cd980</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>Description</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
							<dcsset:viewMode>Normal</dcsset:viewMode>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
