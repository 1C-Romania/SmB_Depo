				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>false</DynamicDataRead>
				<QueryText>SELECT
	Counterparties.Ref,
	Counterparties.Code,
	Counterparties.TIN,
	Counterparties.KPP,
	Counterparties.Description,
	Counterparties.DescriptionFull
