	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentJournalTimeTrackingDocuments.Ref,
	DocumentJournalTimeTrackingDocuments.Date,
	DocumentJournalTimeTrackingDocuments.DeletionMark,
	DocumentJournalTimeTrackingDocuments.Number,
	DocumentJournalTimeTrackingDocuments.Posted,
	DocumentJournalTimeTrackingDocuments.Company,
	DocumentJournalTimeTrackingDocuments.Department,
	DocumentJournalTimeTrackingDocuments.Employee,
	DocumentJournalTimeTrackingDocuments.Comment,
	DocumentJournalTimeTrackingDocuments.Type
FROM
	DocumentJournal.TimeTrackingDocuments AS DocumentJournalTimeTrackingDocuments
WHERE
	(&amp;Customer = UNDEFINED
			OR &amp;Customer = VALUE(Catalog.Counterparties.EmptyRef)
			OR &amp;Customer = VALUE(Catalog.CounterpartyContracts.EmptyRef)
			OR &amp;Customer = VALUE(Document.CustomerOrder.EmptyRef)
			OR DocumentJournalTimeTrackingDocuments.Ref In
				(SELECT
					NestedSelect.Document AS Ref
				FROM
					(SELECT
						TimeTrackingOperations.Ref AS Document
					FROM
						Document.TimeTracking.Operations AS TimeTrackingOperations
					WHERE
						TimeTrackingOperations.Customer = &amp;Customer
		
					UNION ALL
		
					SELECT
						WorkOrdersWorks.Ref AS Document
					FROM
						Document.WorkOrder.Works AS WorkOrdersWorks
					WHERE
						WorkOrdersWorks.Customer = &amp;Customer
					) AS NestedSelect))</QueryText>
				<MainTable>DocumentJournal.TimeTrackingDocuments</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
