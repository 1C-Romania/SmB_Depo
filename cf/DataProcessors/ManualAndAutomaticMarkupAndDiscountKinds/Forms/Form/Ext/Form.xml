		</Attribute>
		<Attribute name="AutomaticDiscounts" id="3">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Automatic discounts</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Автоматические скидки</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<UseAlways>
				<Field>AutomaticDiscounts.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogAutomaticDiscounts.Description,
	CASE
		WHEN CatalogAutomaticDiscounts.IsFolder
			THEN CASE
					WHEN CatalogAutomaticDiscounts.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Max)
						THEN 8
					WHEN CatalogAutomaticDiscounts.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Minimum)
						THEN 16
					WHEN CatalogAutomaticDiscounts.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Addition)
						THEN 0
					WHEN CatalogAutomaticDiscounts.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Multiplication)
						THEN 4
					WHEN CatalogAutomaticDiscounts.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Exclusion)
						THEN 12
				END + CASE
					WHEN CatalogAutomaticDiscounts.DeletionMark
						THEN 3
					ELSE 0
				END
		ELSE CASE
				WHEN CatalogAutomaticDiscounts.AssignmentMethod = VALUE(Enum.DiscountsMarkupsProvidingWays.Percent)
					THEN CASE
							WHEN CatalogAutomaticDiscounts.DiscountMarkupValue &lt; 0
								THEN 32
							ELSE 28
						END
				WHEN CatalogAutomaticDiscounts.AssignmentMethod = VALUE(Enum.DiscountsMarkupsProvidingWays.Amount)
					THEN CASE
							WHEN CatalogAutomaticDiscounts.DiscountMarkupValue &lt; 0
								THEN 40
							ELSE 44
						END
			END + CASE
				WHEN CatalogAutomaticDiscounts.DeletionMark
					THEN 3
				ELSE 0
			END
	END AS Picture,
	CatalogAutomaticDiscounts.AdditionalOrderingAttribute AS AdditionalOrderingAttribute,
	CatalogAutomaticDiscounts.DeletionMark,
	CatalogAutomaticDiscounts.Acts,
	CatalogAutomaticDiscounts.Ref,
	CatalogAutomaticDiscounts.Parent,
	CatalogAutomaticDiscounts.AssignmentMethod,
	CatalogAutomaticDiscounts.Purpose,
	CatalogAutomaticDiscounts.AssignmentArea,
	CatalogAutomaticDiscounts.Code,
	CatalogAutomaticDiscounts.ThereAreFoldersToBeClarifiedByProductsAndServices,
	CASE
		WHEN CatalogAutomaticDiscounts.Parent.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Exclusion)
				OR CatalogAutomaticDiscounts.Parent.SharedUsageVariant = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Multiplication)
				OR CatalogAutomaticDiscounts.Parent = VALUE(Catalog.AutomaticDiscounts.EmptyRef)
					AND (DiscountsMarkupsSharedUsageOptions.Value = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Exclusion)
						OR DiscountsMarkupsSharedUsageOptions.Value = VALUE(Enum.DiscountsMarkupsSharedUsageOptions.Multiplication))
			THEN TRUE
		ELSE FALSE
	END AS ColorYellow,
	CatalogAutomaticDiscounts.Comment
	FROM
	Catalog.AutomaticDiscounts AS CatalogAutomaticDiscounts,
		Constant.DiscountsMarkupsSharedUsageOptions AS DiscountsMarkupsSharedUsageOptions</QueryText>
				<MainTable>Catalog.AutomaticDiscounts</MainTable>
				<AutoSaveUserSettings>false</AutoSaveUserSettings>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
