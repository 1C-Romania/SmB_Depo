	<Attributes>
		<Attribute name="OutgoingList" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Outgoing list</v8:content>
				</v8:item>
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Список исходящих</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	RandomED.Ref,
	RandomED.DeletionMark,
	RandomED.Number,
	RandomED.Date,
	RandomED.Posted,
	RandomED.Company,
	RandomED.Counterparty,
	CAST(RandomED.Text AS String(100)) AS CoveringNoteToAttachment,
	RandomED.Direction,
	RandomED.WasRead,
	RandomED.PointInTime,
	EDStates.EDVersionState,
	RandomED.DocumentStatus,
	RandomED.DocumentType
FROM
	Document.RandomED AS RandomED
		LEFT JOIN InformationRegister.EDStates AS EDStates
		ON (EDStates.ObjectReference = RandomED.Ref)
WHERE
	RandomED.Direction = VALUE(Enum.EDDirections.Outgoing)</QueryText>
				<MainTable>Document.RandomED</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:use>false</dcsset:use>
							<dcsset:field>Date</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
						</dcsset:item>
