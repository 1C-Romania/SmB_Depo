		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN InventoryMotionScheduleTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryMotionScheduleTurnovers.Recorder
	END AS Recorder,
	InventoryMotionScheduleTurnovers.SecondPeriod,
	InventoryMotionScheduleTurnovers.MinutePeriod,
	InventoryMotionScheduleTurnovers.HourPeriod,
	InventoryMotionScheduleTurnovers.DayPeriod,
	InventoryMotionScheduleTurnovers.WeekPeriod,
	InventoryMotionScheduleTurnovers.TenDaysPeriod,
	InventoryMotionScheduleTurnovers.MonthPeriod,
	InventoryMotionScheduleTurnovers.QuarterPeriod,
	InventoryMotionScheduleTurnovers.HalfYearPeriod,
	InventoryMotionScheduleTurnovers.YearPeriod,
	InventoryMotionScheduleTurnovers.Company,
	InventoryMotionScheduleTurnovers.MovementType,
	InventoryMotionScheduleTurnovers.Order,
	InventoryMotionScheduleTurnovers.ProductsAndServices,
	InventoryMotionScheduleTurnovers.Characteristic,
	InventoryMotionScheduleTurnovers.QuantityTurnover
	FROM
		AccumulationRegister.InventoryTransferSchedule.Turnovers(, , Auto, ) AS InventoryMotionScheduleTurnovers</query>
