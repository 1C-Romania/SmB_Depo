		<query>SELECT ALLOWED
	CASE
		WHEN InventoryInWarehousesBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryInWarehousesBalanceAndTurnovers.Recorder
	END AS Recorder,
	InventoryInWarehousesBalanceAndTurnovers.SecondPeriod,
	InventoryInWarehousesBalanceAndTurnovers.MinutePeriod,
	InventoryInWarehousesBalanceAndTurnovers.HourPeriod,
	InventoryInWarehousesBalanceAndTurnovers.DayPeriod,
	InventoryInWarehousesBalanceAndTurnovers.WeekPeriod,
	InventoryInWarehousesBalanceAndTurnovers.TenDaysPeriod,
	InventoryInWarehousesBalanceAndTurnovers.MonthPeriod,
	InventoryInWarehousesBalanceAndTurnovers.QuarterPeriod,
	InventoryInWarehousesBalanceAndTurnovers.HalfYearPeriod,
	InventoryInWarehousesBalanceAndTurnovers.YearPeriod,
	InventoryInWarehousesBalanceAndTurnovers.Company,
	InventoryInWarehousesBalanceAndTurnovers.ProductsAndServices,
	InventoryInWarehousesBalanceAndTurnovers.Characteristic,
	InventoryInWarehousesBalanceAndTurnovers.Batch,
	InventoryInWarehousesBalanceAndTurnovers.StructuralUnit,
	InventoryInWarehousesBalanceAndTurnovers.Cell,
	InventoryInWarehousesBalanceAndTurnovers.QuantityOpeningBalance,
	InventoryInWarehousesBalanceAndTurnovers.QuantityClosingBalance,
	InventoryInWarehousesBalanceAndTurnovers.QuantityTurnover,
	InventoryInWarehousesBalanceAndTurnovers.QuantityReceipt,
	InventoryInWarehousesBalanceAndTurnovers.QuantityExpense,
	InventoryInWarehousesBalanceAndTurnovers.ProductsAndServices.MeasurementUnit,
	"Total in all warehouses" AS TotalByWarehouses,
	InventoryInWarehousesBalanceAndTurnovers.SerialNumber
FROM
	AccumulationRegister.SerialNumbers.BalanceAndTurnovers(, , Auto, , ) AS InventoryInWarehousesBalanceAndTurnovers</query>
