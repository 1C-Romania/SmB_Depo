		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN PurchaseOrdersBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE PurchaseOrdersBalanceAndTurnovers.Recorder
	END AS Recorder,
	PurchaseOrdersBalanceAndTurnovers.SecondPeriod,
	PurchaseOrdersBalanceAndTurnovers.MinutePeriod,
	PurchaseOrdersBalanceAndTurnovers.HourPeriod,
	PurchaseOrdersBalanceAndTurnovers.DayPeriod,
	PurchaseOrdersBalanceAndTurnovers.WeekPeriod,
	PurchaseOrdersBalanceAndTurnovers.TenDaysPeriod,
	PurchaseOrdersBalanceAndTurnovers.MonthPeriod,
	PurchaseOrdersBalanceAndTurnovers.QuarterPeriod,
	PurchaseOrdersBalanceAndTurnovers.HalfYearPeriod,
	PurchaseOrdersBalanceAndTurnovers.YearPeriod,
	PurchaseOrdersBalanceAndTurnovers.Company,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder,
	PurchaseOrdersBalanceAndTurnovers.ProductsAndServices,
	PurchaseOrdersBalanceAndTurnovers.Characteristic,
	PurchaseOrdersBalanceAndTurnovers.QuantityOpeningBalance,
	PurchaseOrdersBalanceAndTurnovers.QuantityClosingBalance,
	PurchaseOrdersBalanceAndTurnovers.QuantityTurnover,
	PurchaseOrdersBalanceAndTurnovers.QuantityReceipt,
	PurchaseOrdersBalanceAndTurnovers.QuantityExpense,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.Counterparty AS Counterparty,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.OrderState AS OrderState,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.OrderState.OrderStatus AS OrderStatus,
	PurchaseOrdersBalanceAndTurnovers.PurchaseOrder.Closed AS Closed
	FROM
		AccumulationRegister.PurchaseOrders.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS PurchaseOrdersBalanceAndTurnovers</query>
