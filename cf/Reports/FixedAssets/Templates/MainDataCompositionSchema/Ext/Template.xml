		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN FixedAssetsBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE FixedAssetsBalanceAndTurnovers.Recorder
	END AS Recorder,
	FixedAssetsBalanceAndTurnovers.SecondPeriod,
	FixedAssetsBalanceAndTurnovers.DayPeriod,
	FixedAssetsBalanceAndTurnovers.MonthPeriod,
	FixedAssetsBalanceAndTurnovers.Company,
	FixedAssetsBalanceAndTurnovers.FixedAsset,
	FixedAssetsBalanceAndTurnovers.CostOpeningBalance,
	FixedAssetsBalanceAndTurnovers.CostClosingBalance,
	FixedAssetsBalanceAndTurnovers.CostTurnover,
	FixedAssetsBalanceAndTurnovers.CostReceipt,
	FixedAssetsBalanceAndTurnovers.CostExpense,
	FixedAssetsBalanceAndTurnovers.DepreciationOpeningBalance,
	FixedAssetsBalanceAndTurnovers.DepreciationClosingBalance,
	FixedAssetsBalanceAndTurnovers.DepreciationTurnover,
	FixedAssetsBalanceAndTurnovers.DepreciationReceipt,
	FixedAssetsBalanceAndTurnovers.DepreciationExpense,
	FixedAssetsBalanceAndTurnovers.FixedAsset.GLAccount AS GLAccount,
	FixedAssetsBalanceAndTurnovers.FixedAsset.DepreciationAccount AS DepreciationAccount,
	FixedAssetsParametersSliceLast.StructuralUnit AS Department,
	FixedAssetsBalanceAndTurnovers.CostReceipt - FixedAssetsBalanceAndTurnovers.CostExpense AS CostChanging,
	FixedAssetsBalanceAndTurnovers.CostClosingBalance - FixedAssetsBalanceAndTurnovers.DepreciationClosingBalance AS ResidualCost,
	FixedAssetsBalanceAndTurnovers.FixedAsset.Code AS InventoryNumber,
	FixedAssetsBalanceAndTurnovers.FixedAsset.DepreciationMethod AS DepreciationMethod,
	FixedAssetsBalanceAndTurnovers.FixedAsset.InitialCost AS InitialCost,
	FixedAssetsBalanceAndTurnovers.MinutePeriod,
	FixedAssetsBalanceAndTurnovers.HourPeriod,
	FixedAssetsBalanceAndTurnovers.WeekPeriod,
	FixedAssetsBalanceAndTurnovers.TenDaysPeriod,
	FixedAssetsBalanceAndTurnovers.QuarterPeriod,
	FixedAssetsBalanceAndTurnovers.HalfYearPeriod,
	FixedAssetsBalanceAndTurnovers.YearPeriod
FROM
	AccumulationRegister.FixedAssets.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS FixedAssetsBalanceAndTurnovers
		LEFT JOIN InformationRegister.FixedAssetsParameters.SliceLast AS FixedAssetsParametersSliceLast
		ON FixedAssetsBalanceAndTurnovers.Company = FixedAssetsParametersSliceLast.Company
			AND FixedAssetsBalanceAndTurnovers.FixedAsset = FixedAssetsParametersSliceLast.FixedAsset</query>
	</dataSet>
