				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>pl</v8:lang>
						<v8:content>Initial document number</v8:content>
					</v8:item>
				</title>
			</field>
			<dataSource>DataSource1</dataSource>
			<query>SELECT
	BookkeepingRecordsWithExtDimensions.Period AS Period,
	BookkeepingRecordsWithExtDimensions.Recorder AS Recorder,
	BookkeepingRecordsWithExtDimensions.Account AS Account,
	BookkeepingRecordsWithExtDimensions.ExtDimensionType1,
	BookkeepingRecordsWithExtDimensions.ExtDimensionType2,
	BookkeepingRecordsWithExtDimensions.ExtDimensionType3,
	BookkeepingRecordsWithExtDimensions.AmountDr,
	BookkeepingRecordsWithExtDimensions.AmountCr,
	BookkeepingRecordsWithExtDimensions.RecordType,
	BookkeepingRecordsWithExtDimensions.ExtDimension1,
	BookkeepingRecordsWithExtDimensions.ExtDimension2,
	BookkeepingRecordsWithExtDimensions.ExtDimension3,
	BookkeepingRecordsWithExtDimensions.Currency,
	BookkeepingRecordsWithExtDimensions.CurrencyAmount,
	BookkeepingRecordsWithExtDimensions.Description,
	CASE
		WHEN BookkeepingRecordsWithExtDimensions.Recorder REFS Document.BookkeepingOperation
			THEN CASE
					WHEN BookkeepingRecordsWithExtDimensions.Recorder.OperationType = VALUE(Enum.OperationTypesBookkeepingOperation.BasedOnDocument)
						THEN BookkeepingRecordsWithExtDimensions.Recorder.DocumentBase
					WHEN BookkeepingRecordsWithExtDimensions.Recorder.OperationType = VALUE(Enum.OperationTypesBookkeepingOperation.Any)
							OR BookkeepingRecordsWithExtDimensions.Recorder.OperationType = VALUE(Enum.OperationTypesBookkeepingOperation.AnyWithRecordsGeneration)
						THEN BookkeepingRecordsWithExtDimensions.Recorder.InitialDocumentNumber
					ELSE ""
				END
		ELSE ""
	END AS InitialDocumentNumber,
	CASE
		WHEN BookkeepingRecordsWithExtDimensions.Recorder REFS Document.BookkeepingOperation
			THEN CASE
					WHEN BookkeepingRecordsWithExtDimensions.Recorder.OperationType = VALUE(Enum.OperationTypesBookkeepingOperation.BasedOnDocument)
						THEN BookkeepingRecordsWithExtDimensions.Recorder.DocumentBase
					WHEN BookkeepingRecordsWithExtDimensions.Recorder.OperationType = VALUE(Enum.OperationTypesBookkeepingOperation.Any)
							OR BookkeepingRecordsWithExtDimensions.Recorder.OperationType = VALUE(Enum.OperationTypesBookkeepingOperation.AnyWithRecordsGeneration)
						THEN BookkeepingRecordsWithExtDimensions.Recorder.InitialDocumentDate
					ELSE ""
				END
		ELSE ""
	END AS InitialDocumentDate
FROM
	AccountingRegister.Bookkeeping.RecordsWithExtDimensions(
			{(&amp;BeginOfPeriod)},
			{(&amp;EndOfPeriodBoundary)},
			Account = &amp;Account
				AND Company = &amp;Company,
			,
			) AS BookkeepingRecordsWithExtDimensions

ORDER BY
	Period</query>
