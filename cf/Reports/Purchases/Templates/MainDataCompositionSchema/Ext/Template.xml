		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN PurchasingTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE PurchasingTurnovers.Recorder
	END AS Recorder,
	PurchasingTurnovers.SecondPeriod,
	PurchasingTurnovers.MinutePeriod,
	PurchasingTurnovers.HourPeriod,
	PurchasingTurnovers.DayPeriod,
	PurchasingTurnovers.WeekPeriod,
	PurchasingTurnovers.TenDaysPeriod,
	PurchasingTurnovers.MonthPeriod,
	PurchasingTurnovers.QuarterPeriod,
	PurchasingTurnovers.HalfYearPeriod,
	PurchasingTurnovers.YearPeriod,
	PurchasingTurnovers.Company,
	PurchasingTurnovers.PurchaseOrder,
	PurchasingTurnovers.ProductsAndServices,
	PurchasingTurnovers.Characteristic,
	PurchasingTurnovers.Batch,
	PurchasingTurnovers.Document,
	PurchasingTurnovers.VATRate,
	PurchasingTurnovers.QuantityTurnover AS Count,
	PurchasingTurnovers.AmountTurnover AS Amount,
	PurchasingTurnovers.VATAmountTurnover AS VATAmount,
	PurchasingTurnovers.PurchaseOrder.Counterparty AS Counterparty,
	PurchasingTurnovers.PurchaseOrder.Contract AS Contract
	FROM
		AccumulationRegister.Purchases.Turnovers(, , Auto, ) AS PurchasingTurnovers</query>
