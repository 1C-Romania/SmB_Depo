		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN SettlementsOnOtherOperationsBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE SettlementsOnOtherOperationsBalanceAndTurnovers.Recorder
	END AS Recorder,
	SettlementsOnOtherOperationsBalanceAndTurnovers.SecondPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.MinutePeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.HourPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.DayPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.WeekPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.TenDaysPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.MonthPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.QuarterPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.HalfYearPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.YearPeriod,
	SettlementsOnOtherOperationsBalanceAndTurnovers.Company,
	SettlementsOnOtherOperationsBalanceAndTurnovers.Counterparty,
	SettlementsOnOtherOperationsBalanceAndTurnovers.Contract,
	SettlementsOnOtherOperationsBalanceAndTurnovers.Contract.SettlementsCurrency AS Currency,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountOpeningBalance,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountClosingBalance,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountReceipt,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountExpense,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountCurOpeningBalance,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountCurClosingBalance,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountCurReceipt,
	SettlementsOnOtherOperationsBalanceAndTurnovers.AmountCurExpense,
	SettlementsOnOtherOperationsBalanceAndTurnovers.GLAccount
FROM
	AccumulationRegister.SettlementsWithOtherCounterparties.BalanceAndTurnovers(, , Auto, , ) AS SettlementsOnOtherOperationsBalanceAndTurnovers</query>
