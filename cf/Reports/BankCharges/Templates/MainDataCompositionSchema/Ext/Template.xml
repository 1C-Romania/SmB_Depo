		<dataSource>DataSource1</dataSource>
		<query>SELECT
	CASE
		WHEN BankChargesTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE BankChargesTurnovers.Recorder
	END AS Регистратор,
	BankChargesTurnovers.Recorder,
	BankChargesTurnovers.Company,
	BankChargesTurnovers.BankAccount,
	BankChargesTurnovers.Currency,
	BankChargesTurnovers.BankCharge,
	BankChargesTurnovers.AmountTurnover,
	BankChargesTurnovers.AmountCurTurnover,
	BankChargesTurnovers.SecondPeriod,
	BankChargesTurnovers.MinutePeriod,
	BankChargesTurnovers.HourPeriod,
	BankChargesTurnovers.DayPeriod,
	BankChargesTurnovers.WeekPeriod,
	BankChargesTurnovers.TenDaysPeriod,
	BankChargesTurnovers.MonthPeriod,
	BankChargesTurnovers.QuarterPeriod,
	BankChargesTurnovers.HalfYearPeriod,
	BankChargesTurnovers.YearPeriod
FROM
	AccumulationRegister.BankCharges.Turnovers(, , Auto, ) AS BankChargesTurnovers</query>
