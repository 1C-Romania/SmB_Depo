		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN ProductReleaseTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE ProductReleaseTurnovers.Recorder
	END AS Recorder,
	ProductReleaseTurnovers.SecondPeriod,
	ProductReleaseTurnovers.MinutePeriod,
	ProductReleaseTurnovers.HourPeriod,
	ProductReleaseTurnovers.DayPeriod,
	ProductReleaseTurnovers.WeekPeriod,
	ProductReleaseTurnovers.TenDaysPeriod,
	ProductReleaseTurnovers.MonthPeriod,
	ProductReleaseTurnovers.QuarterPeriod,
	ProductReleaseTurnovers.HalfYearPeriod,
	ProductReleaseTurnovers.YearPeriod,
	ProductReleaseTurnovers.Company,
	ProductReleaseTurnovers.StructuralUnit,
	ProductReleaseTurnovers.ProductsAndServices,
	ProductReleaseTurnovers.Characteristic,
	ProductReleaseTurnovers.Batch,
	ProductReleaseTurnovers.CustomerOrder,
	ProductReleaseTurnovers.Specification,
	ProductReleaseTurnovers.QuantityTurnover AS Count,
	ProductReleaseTurnovers.ProductsAndServices.MeasurementUnit AS MeasurementUnit
	FROM
		AccumulationRegister.ProductRelease.Turnovers(, , Auto, ) AS ProductReleaseTurnovers</query>
