		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	Employees.Parent AS GroupEmployee,
	Employees.Code AS TabNumber,
	Employees.Ref AS Employee,
	Employees.Ind,
	Employees.OccupationType,
	Employees.Ind.Parent AS GroupNaturalPersons,
	Employees.Ind.BirthDate AS BirthDate,
	Employees.Ind.Gender AS Gender,
	TabPostalAddress.Presentation AS IndividualPostalAddress,
	TabActualAddress.Presentation AS IndividualActualAddress,
	TabAnother.Presentation AS OtherContactInformation,
	TabPhone.Presentation AS Phone,
	EmployeesSliceLast.Recorder AS PersonnelRecordsRecorder,
	EmployeesSliceLast.Company,
	EmployeesSliceLast.StructuralUnit AS Department,
	EmployeesSliceLast.Position,
	EmployeesSliceLast.OccupiedRates,
	EmployeesSliceLast.WorkSchedule,
	EmployeesSliceLast.Period AS PeriodStaff,
	CASE
		WHEN EmployeesSliceLast.StructuralUnit IS NULL
				OR EmployeesSliceLast.StructuralUnit = VALUE(Catalog.StructuralUnits.EmptyRef)
			THEN FALSE
		ELSE TRUE
	END AS Works,
	IndividualsDescriptionFullSliceLast.Surname,
	IndividualsDescriptionFullSliceLast.Name,
	IndividualsDescriptionFullSliceLast.Patronymic,
	IndividualsDocumentsSliceLast.Period AS IssueDate,
	IndividualsDocumentsSliceLast.DocumentKind,
	IndividualsDocumentsSliceLast.Series,
	IndividualsDocumentsSliceLast.Number,
	IndividualsDocumentsSliceLast.WhoIssued,
	IndividualsDocumentsSliceLast.DepartmentCode,
	IndividualsDocumentsSliceLast.IsIdentityDocument
FROM
	Catalog.Employees AS Employees
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabActualAddress
		ON Employees.Ind.Ref = TabActualAddress.Ref
			AND (TabActualAddress.Type = VALUE(Catalog.ContactInformationKinds.IndividualActualAddress))
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabPostalAddress
		ON Employees.Ind.Ref = TabPostalAddress.Ref
			AND (TabPostalAddress.Type = VALUE(Catalog.ContactInformationKinds.IndividualPostalAddress))
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabPhone
		ON Employees.Ind.Ref = TabPhone.Ref
			AND (TabPhone.Type = VALUE(Catalog.ContactInformationKinds.IndividualPhone))
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabAnother
		ON Employees.Ind.Ref = TabAnother.Ref
			AND (TabAnother.Type = VALUE(Catalog.ContactInformationKinds.IndividualOtherInformation))
		LEFT JOIN InformationRegister.Employees.SliceLast AS EmployeesSliceLast
		ON Employees.Ref = EmployeesSliceLast.Employee
		LEFT JOIN InformationRegister.IndividualsDescriptionFull.SliceLast AS IndividualsDescriptionFullSliceLast
		ON Employees.Ind = IndividualsDescriptionFullSliceLast.Ind
		LEFT JOIN InformationRegister.IndividualsDocuments.SliceLast AS IndividualsDocumentsSliceLast
		ON Employees.Ind = IndividualsDocumentsSliceLast.Ind</query>
