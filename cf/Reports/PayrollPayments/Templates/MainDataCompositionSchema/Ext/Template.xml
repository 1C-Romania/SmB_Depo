		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN PayrollPaymentsBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE PayrollPaymentsBalanceAndTurnovers.Recorder
	END AS Recorder,
	PayrollPaymentsBalanceAndTurnovers.SecondPeriod,
	PayrollPaymentsBalanceAndTurnovers.MinutePeriod,
	PayrollPaymentsBalanceAndTurnovers.HourPeriod,
	PayrollPaymentsBalanceAndTurnovers.DayPeriod,
	PayrollPaymentsBalanceAndTurnovers.WeekPeriod,
	PayrollPaymentsBalanceAndTurnovers.TenDaysPeriod,
	PayrollPaymentsBalanceAndTurnovers.MonthPeriod,
	PayrollPaymentsBalanceAndTurnovers.QuarterPeriod,
	PayrollPaymentsBalanceAndTurnovers.HalfYearPeriod,
	PayrollPaymentsBalanceAndTurnovers.YearPeriod,
	PayrollPaymentsBalanceAndTurnovers.Company,
	PayrollPaymentsBalanceAndTurnovers.StructuralUnit AS Department,
	PayrollPaymentsBalanceAndTurnovers.Employee,
	PayrollPaymentsBalanceAndTurnovers.Currency,
	PayrollPaymentsBalanceAndTurnovers.RegistrationPeriod,
	PayrollPaymentsBalanceAndTurnovers.AmountOpeningBalance,
	PayrollPaymentsBalanceAndTurnovers.AmountClosingBalance,
	PayrollPaymentsBalanceAndTurnovers.AmountTurnover,
	PayrollPaymentsBalanceAndTurnovers.AmountReceipt,
	PayrollPaymentsBalanceAndTurnovers.AmountExpense,
	PayrollPaymentsBalanceAndTurnovers.AmountCurOpeningBalance,
	PayrollPaymentsBalanceAndTurnovers.AmountCurClosingBalance,
	PayrollPaymentsBalanceAndTurnovers.AmountCurTurnover,
	PayrollPaymentsBalanceAndTurnovers.AmountCurReceipt,
	PayrollPaymentsBalanceAndTurnovers.AmountCurExpense,
	PayrollPaymentsBalanceAndTurnovers.Employee.Ind AS Ind,
	PayrollPaymentsBalanceAndTurnovers.Employee.OccupationType AS OccupationType,
	PayrollPaymentsBalanceAndTurnovers.Employee.Code AS TabNumber
FROM
	AccumulationRegister.PayrollPayments.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS PayrollPaymentsBalanceAndTurnovers</query>
