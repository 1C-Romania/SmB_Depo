		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	AccountsPayableBalances.Company,
	AccountsPayableBalances.Counterparty,
	AccountsPayableBalances.Contract,
	AccountsPayableBalances.Document,
	AccountsPayableBalances.Document.Date AS DateAccountingDocument,
	AccountsPayableBalances.Order,
	AccountsPayableBalances.Contract.SettlementsCurrency AS Currency,
	AccountsPayableBalances.SettlementsType,
	AccountsPayableBalances.Contract.VendorPaymentDueDate AS VendorPaymentDueDate,
	AccountsPayableBalances.AmountBalance,
	AccountsPayableBalances.AmountCurBalance
INTO Tu_AccountsPayable
FROM
	AccumulationRegister.AccountsPayable.Balance AS AccountsPayableBalances
WHERE
	AccountsPayableBalances.Document &lt;&gt; UNDEFINED
	AND AccountsPayableBalances.AmountBalance &gt; 0
	AND AccountsPayableBalances.SettlementsType = VALUE(Enum.SettlementsTypes.Debt)
	AND DATEDIFF(AccountsPayableBalances.Document.Date, &amp;Period, Day) &gt;= 0
	AND AccountsPayableBalances.AmountCurBalance &gt; 0
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	AccountsPayable.Company,
	AccountsPayable.Counterparty,
	AccountsPayable.Contract,
	AccountsPayable.Document,
	AccountsPayable.Order,
	AccountsPayable.Contract.SettlementsCurrency AS Currency,
	AccountsPayable.SettlementsType,
	AccountsPayable.AmountBalance AS Debt,
	CASE
		WHEN AccountsPayable.VendorPaymentDueDate &gt; 0
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; AccountsPayable.VendorPaymentDueDate
			THEN AccountsPayable.AmountBalance
		ELSE 0
	END AS DOverdueDebt,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period1
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt;= 0
			THEN AccountsPayable.AmountBalance
		ELSE 0
	END AS D1,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period2
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period1
			THEN AccountsPayable.AmountBalance
		ELSE 0
	END AS D2,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period3
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period2
			THEN AccountsPayable.AmountBalance
		ELSE 0
	END AS D3,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period4
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period3
			THEN AccountsPayable.AmountBalance
		ELSE 0
	END AS D4,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period4
			THEN AccountsPayable.AmountBalance
		ELSE 0
	END AS D5,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period1
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt;= 0
			THEN AccountsPayable.AmountCurBalance
		ELSE 0
	END AS DV1,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period2
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period1
			THEN AccountsPayable.AmountCurBalance
		ELSE 0
	END AS DV2,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period3
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period2
			THEN AccountsPayable.AmountCurBalance
		ELSE 0
	END AS DOD,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &lt;= &amp;Period4
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period3
			THEN AccountsPayable.AmountCurBalance
		ELSE 0
	END AS DV4,
	CASE
		WHEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; &amp;Period4
			THEN AccountsPayable.AmountCurBalance
		ELSE 0
	END AS DV5,
	AccountsPayable.AmountCurBalance AS DebtCur,
	CASE
		WHEN AccountsPayable.VendorPaymentDueDate &gt; 0
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; AccountsPayable.VendorPaymentDueDate
			THEN AccountsPayable.AmountCurBalance
		ELSE 0
	END AS DVOverdueDebt,
	CASE
		WHEN AccountsPayable.VendorPaymentDueDate &gt; 0
				AND DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) &gt; AccountsPayable.VendorPaymentDueDate
			THEN DATEDIFF(AccountsPayable.DateAccountingDocument, &amp;Period, Day) - AccountsPayable.VendorPaymentDueDate
		ELSE 0
	END AS DelayTerm
FROM
	Tu_AccountsPayable AS AccountsPayable</query>
