		<query>SELECT ALLOWED
	CASE
		WHEN OrdersForProductionOfBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE OrdersForProductionOfBalanceAndTurnovers.Recorder
	END AS Recorder,
	OrdersForProductionOfBalanceAndTurnovers.SecondPeriod,
	OrdersForProductionOfBalanceAndTurnovers.MinutePeriod,
	OrdersForProductionOfBalanceAndTurnovers.HourPeriod,
	OrdersForProductionOfBalanceAndTurnovers.DayPeriod,
	OrdersForProductionOfBalanceAndTurnovers.WeekPeriod,
	OrdersForProductionOfBalanceAndTurnovers.TenDaysPeriod,
	OrdersForProductionOfBalanceAndTurnovers.MonthPeriod,
	OrdersForProductionOfBalanceAndTurnovers.QuarterPeriod,
	OrdersForProductionOfBalanceAndTurnovers.HalfYearPeriod,
	OrdersForProductionOfBalanceAndTurnovers.YearPeriod,
	OrdersForProductionOfBalanceAndTurnovers.Company,
	OrdersForProductionOfBalanceAndTurnovers.ProductionOrder,
	OrdersForProductionOfBalanceAndTurnovers.ProductsAndServices,
	OrdersForProductionOfBalanceAndTurnovers.Characteristic,
	OrdersForProductionOfBalanceAndTurnovers.QuantityOpeningBalance,
	OrdersForProductionOfBalanceAndTurnovers.QuantityClosingBalance,
	OrdersForProductionOfBalanceAndTurnovers.QuantityTurnover,
	OrdersForProductionOfBalanceAndTurnovers.QuantityReceipt,
	OrdersForProductionOfBalanceAndTurnovers.QuantityExpense,
	OrdersForProductionOfBalanceAndTurnovers.ProductionOrder.OrderState AS OrderState,
	OrdersForProductionOfBalanceAndTurnovers.ProductionOrder.OrderState.OrderStatus AS OrderStatus,
	OrdersForProductionOfBalanceAndTurnovers.ProductionOrder.Closed AS Closed,
	OrdersForProductionOfBalanceAndTurnovers.ProductionOrder.StructuralUnit AS Manufacturer
	FROM
		AccumulationRegister.ProductionOrders.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS OrdersForProductionOfBalanceAndTurnovers</query>
