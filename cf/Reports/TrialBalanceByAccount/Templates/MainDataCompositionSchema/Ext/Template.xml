		<dataSource>DataSource1</dataSource>
		<query>SELECT
	BookkeepingBalanceAndTurnoversYear.Account,
	BookkeepingBalanceAndTurnoversYear.ExtDimension1,
	BookkeepingBalanceAndTurnoversYear.ExtDimension2,
	BookkeepingBalanceAndTurnoversYear.ExtDimension3,
	ISNULL(BookkeepingBalanceAndTurnoversYear.AmountOpeningSplittedBalanceDr, 0) AS OpeningBalanceDrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.AmountOpeningSplittedBalanceCr, 0) AS OpeningBalanceCrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.AmountTurnoverDr, 0) AS TurnoverDrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.AmountTurnoverCr, 0) AS TurnoverCrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.AmountClosingSplittedBalanceDr, 0) AS ClosingBalanceDrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.AmountClosingSplittedBalanceCr, 0) AS ClosingBalanceCrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.CurrencyAmountOpeningSplittedBalanceDr, 0) AS CurrencyOpeningBalanceDrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.CurrencyAmountOpeningSplittedBalanceCr, 0) AS CurrencyOpeningBalanceCrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.CurrencyAmountTurnoverDr, 0) AS CurrencyTurnoverDrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.CurrencyAmountTurnoverCr, 0) AS CurrencyTurnoverCrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.CurrencyAmountClosingSplittedBalanceDr, 0) AS CurrencyClosingBalanceDrOrNull,
	ISNULL(BookkeepingBalanceAndTurnoversYear.CurrencyAmountClosingSplittedBalanceCr, 0) AS CurrencyClosingBalanceCrOrNull,
	BookkeepingBalanceAndTurnoversYear.Currency
FROM
	AccountingRegister.Bookkeeping.BalanceAndTurnovers({(&amp;BeginOfYear)}, {(&amp;EndOfPeriod)}, , , Account = &amp;Account, , Company = &amp;Company) AS BookkeepingBalanceAndTurnoversYear</query>
	</dataSet>
