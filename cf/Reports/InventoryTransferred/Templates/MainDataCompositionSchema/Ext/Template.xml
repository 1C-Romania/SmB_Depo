		<query>SELECT ALLOWED
	CASE
		WHEN InventoryTransferredBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryTransferredBalancesAndTurnovers.Recorder
	END AS Recorder,
	InventoryTransferredBalancesAndTurnovers.SecondPeriod,
	InventoryTransferredBalancesAndTurnovers.MinutePeriod,
	InventoryTransferredBalancesAndTurnovers.HourPeriod,
	InventoryTransferredBalancesAndTurnovers.DayPeriod,
	InventoryTransferredBalancesAndTurnovers.WeekPeriod,
	InventoryTransferredBalancesAndTurnovers.TenDaysPeriod,
	InventoryTransferredBalancesAndTurnovers.MonthPeriod,
	InventoryTransferredBalancesAndTurnovers.QuarterPeriod,
	InventoryTransferredBalancesAndTurnovers.HalfYearPeriod,
	InventoryTransferredBalancesAndTurnovers.YearPeriod,
	InventoryTransferredBalancesAndTurnovers.Company,
	InventoryTransferredBalancesAndTurnovers.ProductsAndServices,
	InventoryTransferredBalancesAndTurnovers.Characteristic,
	InventoryTransferredBalancesAndTurnovers.Batch,
	InventoryTransferredBalancesAndTurnovers.ReceptionTransmissionType,
	InventoryTransferredBalancesAndTurnovers.QuantityOpeningBalance,
	InventoryTransferredBalancesAndTurnovers.QuantityClosingBalance,
	InventoryTransferredBalancesAndTurnovers.QuantityTurnover,
	InventoryTransferredBalancesAndTurnovers.QuantityReceipt,
	InventoryTransferredBalancesAndTurnovers.QuantityExpense,
	InventoryTransferredBalancesAndTurnovers.SettlementsAmountOpeningBalance,
	InventoryTransferredBalancesAndTurnovers.SettlementsAmountClosingBalance,
	InventoryTransferredBalancesAndTurnovers.SettlementsAmountTurnover,
	InventoryTransferredBalancesAndTurnovers.SettlementsAmountReceipt,
	InventoryTransferredBalancesAndTurnovers.SettlementsAmountExpense,
	InventoryTransferredBalancesAndTurnovers.ProductsAndServices.MeasurementUnit,
	InventoryTransferredBalancesAndTurnovers.Counterparty,
	InventoryTransferredBalancesAndTurnovers.Contract
	FROM
		AccumulationRegister.InventoryTransferred.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS InventoryTransferredBalancesAndTurnovers</query>
	</dataSet>
