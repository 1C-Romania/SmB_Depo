	"Intangible costs" AS ProductsAndServices,
	Inventory.Amount AS ExpensesCost,
	Inventory.Quantity AS CostsQuantity,
	0 AS ProductsQuantity
FROM
	AccumulationRegister.Inventory AS Inventory
WHERE
	Inventory.Recorder REFS Document.SubcontractorReport
	AND Inventory.FixedCost
	AND Inventory.StructuralUnit.StructuralUnitType = VALUE(Enum.StructuralUnitsTypes.Warehouse)
