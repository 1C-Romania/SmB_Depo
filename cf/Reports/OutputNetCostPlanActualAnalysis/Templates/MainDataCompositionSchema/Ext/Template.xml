					</v8:item>
				</title>
			</field>
			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	Inventory.Recorder AS Recorder,
	Inventory.Company AS Company,
	Inventory.StructuralUnit AS Department,
	Inventory.ProductsAndServices AS Products,
	Inventory.Characteristic AS ProductCharacteristic,
