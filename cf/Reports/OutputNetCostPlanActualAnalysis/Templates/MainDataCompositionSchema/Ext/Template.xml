				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>Department</v8:content>
					</v8:item>
				</title>
			</field>
			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	Inventory.Recorder AS Recorder,
	Inventory.Company AS Company,
	Inventory.StructuralUnitCorr AS Department,
	Inventory.ProductsAndServicesCorr AS Products,
	Inventory.CharacteristicCorr AS ProductCharacteristic,
	Inventory.BatchCorr AS BatchProducts,
	Inventory.SpecificationCorr AS ProductionSpecification,
	Inventory.CustomerCorrOrder AS CustomerOrder,
	CASE
		WHEN Inventory.ProductsAndServices = VALUE(Catalog.ProductsAndServices.EmptyRef)
			THEN "Intangible costs"
		ELSE Inventory.ProductsAndServices
	END AS ProductsAndServices,
	Inventory.Characteristic AS Characteristic,
	Inventory.Batch AS Batch,
	Inventory.Amount AS ExpensesCost,
	Inventory.Quantity AS CostsQuantity,
	0 AS ProductsQuantity,
	Inventory.Specification AS Specification
FROM
	AccumulationRegister.Inventory AS Inventory
WHERE
	Inventory.RecordType = VALUE(AccumulationRecordType.Expense)
	AND Inventory.ProductionExpenses
	AND Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod</query>
