		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN NestedSelect.Recorder = UNDEFINED
			THEN NULL
		ELSE NestedSelect.Recorder
	END AS Recorder,
	NestedSelect.SecondPeriod,
	NestedSelect.MinutePeriod,
	NestedSelect.HourPeriod,
	NestedSelect.DayPeriod,
	NestedSelect.WeekPeriod,
	NestedSelect.TenDaysPeriod,
	NestedSelect.MonthPeriod,
	NestedSelect.QuarterPeriod,
	NestedSelect.HalfYearPeriod,
	NestedSelect.YearPeriod,
	NestedSelect.Company,
	NestedSelect.PlanningPeriod,
	NestedSelect.ProductsAndServices,
	NestedSelect.Characteristic,
	NestedSelect.CustomerOrder,
	NestedSelect.MeasurementUnit,
	NestedSelect.QuantityPlan,
	NestedSelect.AmountPlan,
	NestedSelect.AmountVATPlan,
	NestedSelect.AmountWithoutVATPlan,
	NestedSelect.QuantityFact,
	NestedSelect.AmountFact,
	NestedSelect.AmountVATFact,
	NestedSelect.AmountWithoutVATFact,
	ISNULL(NestedSelect.QuantityFact, 0) - ISNULL(NestedSelect.QuantityPlan, 0) AS QuantityRejection,
	ISNULL(NestedSelect.AmountFact, 0) - ISNULL(NestedSelect.AmountPlan, 0) AS AmountRejection,
	ISNULL(NestedSelect.AmountVATFact, 0) - ISNULL(NestedSelect.AmountVATPlan, 0) AS VATAmountRejection,
	ISNULL(NestedSelect.AmountWithoutVATFact, 0) - ISNULL(NestedSelect.AmountWithoutVATPlan, 0) AS AmountWithoutVATRejection,
	0 AS CountRejectionPercent,
	0 AS AmountRejectionPercent,
	0 AS VATAmountRejectionPercent,
	0 AS AmountWithoutVATRejectionPercent,
	NestedSelect.Responsible
	FROM
	(SELECT
		SUM(SalesTargetsTurnovers.LineNumber) AS LineNumber,
		SalesTargetsTurnovers.Recorder AS Recorder,
		SalesTargetsTurnovers.SecondPeriod AS SecondPeriod,
		SalesTargetsTurnovers.MinutePeriod AS MinutePeriod,
		SalesTargetsTurnovers.HourPeriod AS HourPeriod,
		SalesTargetsTurnovers.DayPeriod AS DayPeriod,
		SalesTargetsTurnovers.WeekPeriod AS WeekPeriod,
		SalesTargetsTurnovers.TenDaysPeriod AS TenDaysPeriod,
		SalesTargetsTurnovers.MonthPeriod AS MonthPeriod,
		SalesTargetsTurnovers.QuarterPeriod AS QuarterPeriod,
		SalesTargetsTurnovers.HalfYearPeriod AS HalfYearPeriod,
		SalesTargetsTurnovers.YearPeriod AS YearPeriod,
		SalesTargetsTurnovers.Company AS Company,
		SalesTargetsTurnovers.PlanningPeriod AS PlanningPeriod,
		SalesTargetsTurnovers.ProductsAndServices AS ProductsAndServices,
		SalesTargetsTurnovers.Characteristic AS Characteristic,
		SalesTargetsTurnovers.CustomerOrder AS CustomerOrder,
		SalesTargetsTurnovers.ProductsAndServices.MeasurementUnit AS MeasurementUnit,
		SUM(SalesTargetsTurnovers.QuantityTurnover) AS QuantityPlan,
		SUM(SalesTargetsTurnovers.AmountTurnover) AS AmountPlan,
		SUM(SalesTargetsTurnovers.VATAmountTurnover) AS AmountVATPlan,
		SUM(SalesTargetsTurnovers.AmountTurnover - SalesTargetsTurnovers.VATAmountTurnover) AS AmountWithoutVATPlan,
		0 AS QuantityFact,
		0 AS AmountVATFact,
		0 AS AmountFact,
		0 AS AmountWithoutVATFact,
		SalesTargetsTurnovers.PlanningDocument.Responsible AS Responsible
	FROM
		AccumulationRegister.SalesTargets.Turnovers(, , Auto, PlanningPeriod = &amp;PlanningPeriod
					OR &amp;PlanningPeriod = VALUE(Catalog.PlanningPeriods.EmptyRef)) AS SalesTargetsTurnovers
	
	GROUP BY
		SalesTargetsTurnovers.SecondPeriod,
		SalesTargetsTurnovers.Recorder,
		SalesTargetsTurnovers.MinutePeriod,
		SalesTargetsTurnovers.HourPeriod,
		SalesTargetsTurnovers.DayPeriod,
		SalesTargetsTurnovers.WeekPeriod,
		SalesTargetsTurnovers.TenDaysPeriod,
		SalesTargetsTurnovers.MonthPeriod,
		SalesTargetsTurnovers.QuarterPeriod,
		SalesTargetsTurnovers.HalfYearPeriod,
		SalesTargetsTurnovers.YearPeriod,
		SalesTargetsTurnovers.Company,
		SalesTargetsTurnovers.PlanningPeriod,
		SalesTargetsTurnovers.ProductsAndServices,
		SalesTargetsTurnovers.Characteristic,
		SalesTargetsTurnovers.CustomerOrder,
		SalesTargetsTurnovers.ProductsAndServices.MeasurementUnit,
		SalesTargetsTurnovers.PlanningDocument.Responsible
	
	UNION ALL
	
	SELECT
		SUM(SalesTurnovers.LineNumber),
		SalesTurnovers.Recorder,
		SalesTurnovers.SecondPeriod,
		SalesTurnovers.MinutePeriod,
		SalesTurnovers.HourPeriod,
		SalesTurnovers.DayPeriod,
		SalesTurnovers.WeekPeriod,
		SalesTurnovers.TenDaysPeriod,
		SalesTurnovers.MonthPeriod,
		SalesTurnovers.QuarterPeriod,
		SalesTurnovers.HalfYearPeriod,
		SalesTurnovers.YearPeriod,
		SalesTurnovers.Company,
		NULL,
		SalesTurnovers.ProductsAndServices,
		SalesTurnovers.Characteristic,
		SalesTurnovers.CustomerOrder,
		SalesTurnovers.ProductsAndServices.MeasurementUnit,
		0,
		0,
		0,
		0,
		SUM(SalesTurnovers.QuantityTurnover),
		SUM(SalesTurnovers.VATAmountTurnover),
		SUM(SalesTurnovers.AmountTurnover),
		SUM(SalesTurnovers.AmountTurnover - SalesTurnovers.VATAmountTurnover),
		SalesTurnovers.Recorder.Responsible
	FROM
		AccumulationRegister.Sales.Turnovers(, , Auto, ) AS SalesTurnovers
	
	GROUP BY
		SalesTurnovers.SecondPeriod,
		SalesTurnovers.Recorder,
		SalesTurnovers.MinutePeriod,
		SalesTurnovers.HourPeriod,
		SalesTurnovers.DayPeriod,
		SalesTurnovers.WeekPeriod,
		SalesTurnovers.TenDaysPeriod,
		SalesTurnovers.MonthPeriod,
		SalesTurnovers.QuarterPeriod,
		SalesTurnovers.HalfYearPeriod,
		SalesTurnovers.YearPeriod,
		SalesTurnovers.Company,
		SalesTurnovers.CustomerOrder,
		SalesTurnovers.ProductsAndServices,
		SalesTurnovers.Characteristic,
		SalesTurnovers.ProductsAndServices.MeasurementUnit,
			SalesTurnovers.Recorder.Responsible) AS NestedSelect</query>
	</dataSet>
