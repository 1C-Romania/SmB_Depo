	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogContactPersons.Ref AS Ref,
	CatalogContactPersons.DeletionMark AS DeletionMark,
	CatalogContactPersons.Predefined AS Predefined,
	CatalogContactPersons.Owner AS Owner,
	CatalogContactPersons.Code AS Code,
	CatalogContactPersons.Description AS Description,
	CatalogContactPersons.Ind AS Ind,
	CatalogContactPersons.ConnectionRegistrationDate AS ConnectionRegistrationDate,
	CatalogContactPersons.ConnectionStopDate AS ConnectionStopDate,
	CatalogContactPersons.Responsible AS Responsible,
	CatalogContactPersons.Comment AS Comment,
	CatalogContactPersons.ContactPersonRoles.(
		Ref,
		LineNumber,
		ContactPersonRole
	) AS ContactPersonRoles,
	TableRoles.ContactPersonRole AS ContactPersonRole
FROM
	Catalog.ContactPersons AS CatalogContactPersons
		LEFT JOIN (SELECT
			ContactPersonRolesOfContactPerson.Ref AS Ref,
			ContactPersonRolesOfContactPerson.ContactPersonRole.Description AS ContactPersonRole
		FROM
			Catalog.ContactPersons.ContactPersonRoles AS ContactPersonRolesOfContactPerson
		WHERE
			ContactPersonRolesOfContactPerson.LineNumber = 1) AS TableRoles
			ON CatalogContactPersons.Ref = TableRoles.Ref</QueryText>
				<MainTable>Catalog.ContactPersons</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
