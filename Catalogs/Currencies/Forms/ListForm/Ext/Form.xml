	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Currencies.DeletionMark,
	Currencies.Code,
	Currencies.Description,
	Currencies.DescriptionFull,
	CurrencyRates.ExchangeRate,
	CASE
		WHEN CurrencyRates.Multiplicity = 1
			THEN " "
		ELSE "US $ for"
	END AS CurrencyUnitPer,
	CASE
		WHEN CurrencyRates.Multiplicity = 1
			THEN " "
		ELSE CurrencyRates.Multiplicity
	END AS Multiplicity,
	CASE
		WHEN CurrencyRates.Multiplicity = 1
			THEN " "
		ELSE Currencies.Description
	END AS ShortDescription,
	Currencies.Ref
FROM
	Catalog.Currencies AS Currencies
		LEFT JOIN InformationRegister.CurrencyRates.SliceLast(&amp;EndOfPeriod, ) AS CurrencyRates
			ON Currencies.Ref = CurrencyRates.Currency</QueryText>
				<MainTable>Catalog.Currencies</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>DescriptionFull</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
