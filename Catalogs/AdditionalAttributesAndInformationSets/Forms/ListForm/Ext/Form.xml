	<Attributes>
		<Attribute name="PropertiesSets" id="1">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Properties sets</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>PropertiesSets.Ref</Field>
				<Field>PropertiesSets.Parent</Field>
				<Field>PropertiesSets.IsFolder</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Sets.Ref,
	Sets.DataVersion,
	Sets.DeletionMark,
	Sets.Predefined,
	Sets.Parent,
	Sets.IsFolder,
	Sets.Description,
	CASE
		WHEN Sets.IsFolder
			THEN Sets.Description
		WHEN &amp;ThisIsSetOfAdditionalInformation
			THEN CASE
					WHEN Sets.CountInformation = ""
						THEN Sets.Description
					ELSE Sets.Description + " (" + Sets.CountInformation + ")"
				END
		ELSE CASE
				WHEN Sets.CountAttributes = ""
					THEN Sets.Description
				ELSE Sets.Description + " (" + Sets.CountAttributes + ")"
			END
	END AS Presentation,
	CASE
		WHEN Sets.DeletionMark
			THEN 1
		ELSE 0
	END + CASE
		WHEN Sets.Predefined
			THEN 2
		ELSE 0
	END AS PictureNumber
FROM
	Catalog.AdditionalAttributesAndInformationSets AS Sets
WHERE
	CASE
			WHEN Sets.Parent = VALUE(Catalog.AdditionalAttributesAndInformationSets.EmptyRef)
				THEN Sets.Ref IN (&amp;Sets)
			ELSE Sets.Parent IN (&amp;Sets)
			END</QueryText>
