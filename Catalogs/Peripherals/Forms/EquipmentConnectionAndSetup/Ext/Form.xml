	CatalogPeripherals.Ref,
	CatalogPeripherals.Description,
	CatalogPeripherals.DeviceIsInUse AS Used,
	CatalogPeripherals.HardwareDriver,
	CatalogPeripherals.Workplace,
	CatalogPeripherals.EquipmentType
