	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.StoreVersions</Field>
				<Field>List.Ref</Field>
				<Field>List.IsEditing</Field>
				<Field>List.DigitallySigned</Field>
				<Field>List.CurrentUserIsEditing</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Files.Code,
	Files.IsEditing,
	Files.CreationDate,
	Files.CurrentVersionSize / 1024 AS CurrentVersionSize,
	Files.Author,
	Files.CurrentVersionCreationDate,
	CASE
		WHEN Files.DeletionMark = TRUE
			THEN Files.PictureIndex + 1
		ELSE Files.PictureIndex
	END AS PictureIndex,
	Files.Description,
	Files.Description,
	Files.Ref,
	Files.CurrentVersionAuthor,
	Files.DeletionMark,
	Files.StoreVersions,
	CASE
		WHEN Files.IsEditing = &amp;CurrentUser
			THEN TRUE
		ELSE FALSE
	END AS CurrentUserIsEditing,
	Files.FileOwner,
	Files.CurrentVersionExtension,
	Files.CurrentVersionVersionNumber,
	Files.CurrentVersionVolume,
	Files.CurrentVersionPathToFile,
	Files.DigitallySigned,
	Files.Encrypted,
	CASE
		WHEN Files.DigitallySigned
				AND Files.Encrypted
			THEN 2
		WHEN Files.Encrypted
			THEN 1
		WHEN Files.DigitallySigned
			THEN 0
		ELSE -1
	END AS PictureNumberDigitallySignedEncrypted
FROM
		Catalog.Files AS Files</QueryText>
				<MainTable>Catalog.Files</MainTable>
