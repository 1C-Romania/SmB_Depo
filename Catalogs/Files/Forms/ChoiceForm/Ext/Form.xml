	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
				<Field>List.DigitallySigned</Field>
				<Field>List.CurrentVersionExtension</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Files.Ref,
	Files.DeletionMark,
	Files.Predefined,
	Files.Code,
	Files.Description,
	Files.Author,
	Files.CreationDate,
	Files.StoreVersions,
	Files.IsEditing,
	Files.CurrentVersion,
	Files.PictureIndex,
	Files.Definition,
	Files.FullDescr,
	Files.FileOwner,
	Files.DigitallySigned,
	Files.Encrypted,
	CASE
		WHEN Files.DigitallySigned
				AND Files.Encrypted
			THEN 2
		WHEN Files.Encrypted
			THEN 1
		WHEN Files.DigitallySigned
			THEN 0
		ELSE -1
	END AS PictureNumberDigitallySignedEncrypted,
	Files.CurrentVersionExtension
	FROM
	Catalog.Files AS Files
	WHERE
	Files.FileOwner = &amp;Owner
		AND Files.DeletionMark = FALSE</QueryText>
				<MainTable>Catalog.Files</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
