		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN InventoryTurnover.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryTurnover.Recorder
	END AS Recorder,
	InventoryTurnover.SecondPeriod,
	InventoryTurnover.MinutePeriod,
	InventoryTurnover.HourPeriod,
	InventoryTurnover.DayPeriod,
	InventoryTurnover.WeekPeriod,
	InventoryTurnover.TenDaysPeriod,
	InventoryTurnover.MonthPeriod,
	InventoryTurnover.QuarterPeriod,
	InventoryTurnover.YearPeriod,
	InventoryTurnover.Company,
	InventoryTurnover.StructuralUnit,
	InventoryTurnover.GLAccount,
	InventoryTurnover.ProductsAndServices AS ProductsAndServices,
	InventoryTurnover.Characteristic AS Characteristic,
	InventoryTurnover.Batch,
	InventoryTurnover.ProductsAndServices.MeasurementUnit AS MeasurementUnit,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryReceipt)
			THEN InventoryTurnover.QuantityReceipt
		ELSE -InventoryTurnover.QuantityExpense
	END AS Count,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryReceipt)
			THEN InventoryTurnover.AmountReceipt
		ELSE -InventoryTurnover.AmountExpense
	END AS Amount,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryReceipt)
			THEN "EXCESSES"
		ELSE "SHORTAGE"
	END AS RegradingType,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryReceipt)
				OR VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryWriteOff)
			THEN InventoryTurnover.Recorder.BasisDocument
		ELSE UNDEFINED
	END AS InventoryDocument
	FROM
	AccumulationRegister.Inventory.Turnovers(, , Auto, ) AS InventoryTurnover
	WHERE
	(VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryReceipt)
				OR VALUETYPE(InventoryTurnover.Recorder) = Type(Document.InventoryWriteOff))</query>
