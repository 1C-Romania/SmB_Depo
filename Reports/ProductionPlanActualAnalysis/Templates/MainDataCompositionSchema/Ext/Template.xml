		<dataSource>DataSource1</dataSource>
		<query>SELECT
	ProductReleaseTurnovers.Company AS Company,
	ProductReleaseTurnovers.CustomerOrder AS CustomerOrder,
	ProductReleaseTurnovers.StructuralUnit AS StructuralUnit,
	ProductReleaseTurnovers.ProductsAndServices AS ProductsAndServices,
	ProductReleaseTurnovers.ProductsAndServices.MeasurementUnit AS MeasurementUnit,
	ProductReleaseTurnovers.Characteristic,
	ProductReleaseTurnovers.Specification,
	ProductReleaseTurnovers.Batch,
	ProductReleaseTurnovers.QuantityTurnover AS Produced,
	ProductReleaseTurnovers.QuantityPlanTurnover AS Planned,
	ProductReleaseTurnovers.Recorder,
	ProductReleaseTurnovers.SecondPeriod
FROM
	AccumulationRegister.ProductRelease.Turnovers(, , Auto, ) AS ProductReleaseTurnovers</query>
