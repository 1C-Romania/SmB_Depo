		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	PaymentCalendar.InvoiceForPayment AS CustomerOrder,
	PaymentCalendar.AmountTurnover AS AmountTurnover
INTO TemporaryTablePaymentCalendar
FROM
	AccumulationRegister.PaymentCalendar.Turnovers(
			,
			,
			,
			PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
				AND InvoiceForPayment = &amp;Order) AS PaymentCalendar

UNION ALL

SELECT
	CAST(PaymentCalendar.InvoiceForPayment AS Document.InvoiceForPayment).BasisDocument,
	PaymentCalendar.AmountTurnover
FROM
	AccumulationRegister.PaymentCalendar.Turnovers(
			,
			,
			,
			PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
				AND CAST(InvoiceForPayment AS Document.InvoiceForPayment).BasisDocument = &amp;Order) AS PaymentCalendar

UNION ALL

SELECT
	CAST(PaymentCalendar.InvoiceForPayment AS Document.PaymentReceiptPlan).BasisDocument,
	PaymentCalendar.AmountTurnover
FROM
	AccumulationRegister.PaymentCalendar.Turnovers(
			,
			,
			,
			PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
				AND CAST(InvoiceForPayment AS Document.PaymentReceiptPlan).BasisDocument = &amp;Order) AS PaymentCalendar
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	PaymentCalendar.CustomerOrder AS CustomerOrder,
	SUM(PaymentCalendar.AmountTurnover) AS PaymentPlan
INTO TemporaryTablePlannedPayment
FROM
	TemporaryTablePaymentCalendar AS PaymentCalendar

GROUP BY
	PaymentCalendar.CustomerOrder
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	CustomerOrdersBalanceAndTurnovers.CustomerOrder AS CustomerOrder,
	CustomerOrdersBalanceAndTurnovers.CustomerOrder.DocumentCurrency AS DocumentCurrency,
	CustomerOrdersBalanceAndTurnovers.CustomerOrder.OrderState AS OrderState,
	CustomerOrdersBalanceAndTurnovers.CustomerOrder.Counterparty AS Counterparty,
	CustomerOrdersBalanceAndTurnovers.CustomerOrder.Contract AS Contract,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) AS DocumentAmount,
	ISNULL(Plannedpayment.PaymentPlan, 0) AS PaymentPlan,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) AS Prepayment,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS Payment,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) + ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS Paid,
	ISNULL(InvoicesAndOrdersPaymentTurnovers.AmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover, 0) - ISNULL(InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover, 0) AS LeftToPay
FROM
	AccumulationRegister.CustomerOrders.BalanceAndTurnovers(, , , , CustomerOrder = &amp;Order) AS CustomerOrdersBalanceAndTurnovers
		LEFT JOIN AccumulationRegister.InvoicesAndOrdersPayment.Turnovers(, , , InvoiceForPayment = &amp;Order) AS InvoicesAndOrdersPaymentTurnovers
		ON CustomerOrdersBalanceAndTurnovers.CustomerOrder = InvoicesAndOrdersPaymentTurnovers.InvoiceForPayment
		LEFT JOIN TemporaryTablePlannedPayment AS Plannedpayment
		ON CustomerOrdersBalanceAndTurnovers.CustomerOrder = Plannedpayment.CustomerOrder</query>
