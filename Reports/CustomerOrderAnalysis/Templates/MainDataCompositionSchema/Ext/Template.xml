		<field xsi:type="DataSetFieldField">
			<dataPath>Ordered</dataPath>
			<field>Ordered</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CustomerOrdersBalanceAndTurnovers.ProductsAndServices AS ProductsAndServices,
	CASE
		WHEN CustomerOrdersBalanceAndTurnovers.Characteristic = VALUE(Catalog.ProductsAndServicesCharacteristics.EmptyRef)
			THEN NULL
		ELSE CustomerOrdersBalanceAndTurnovers.Characteristic
	END AS Characteristic,
	CustomerOrdersBalanceAndTurnovers.QuantityReceipt AS Ordered,
	CustomerOrdersBalanceAndTurnovers.QuantityExpense AS Shipped,
	CustomerOrdersBalanceAndTurnovers.QuantityClosingBalance AS RemainingToShip,
	ISNULL(Reserves.QuantityBalance, 0) AS ReservedAtWarehouse,
	ISNULL(OrdersPlacementBalances.QuantityBalance, 0) AS PlacedInOrders,
	ISNULL(AvailableBallance.QuantityBalance, 0) AS AvailableBalance,
	CASE
		WHEN CustomerOrdersBalanceAndTurnovers.ProductsAndServices.ProductsAndServicesType = VALUE(Enum.ProductsAndServicesTypes.InventoryItem)
			THEN CustomerOrdersBalanceAndTurnovers.QuantityClosingBalance - ISNULL(Reserves.QuantityBalance, 0) - ISNULL(OrdersPlacementBalances.QuantityBalance, 0)
		ELSE 0
	END AS LeftToProvide
	FROM
	AccumulationRegister.CustomerOrders.BalanceAndTurnovers(, , , , CustomerOrder = &amp;Order) AS CustomerOrdersBalanceAndTurnovers
		LEFT JOIN AccumulationRegister.Inventory.Balance(, CustomerOrder = &amp;Order) AS Reserves
		ON CustomerOrdersBalanceAndTurnovers.ProductsAndServices = Reserves.ProductsAndServices
			AND CustomerOrdersBalanceAndTurnovers.Characteristic = Reserves.Characteristic
			AND CustomerOrdersBalanceAndTurnovers.CustomerOrder = Reserves.CustomerOrder
			AND CustomerOrdersBalanceAndTurnovers.Company = Reserves.Company
		LEFT JOIN AccumulationRegister.OrdersPlacement.Balance(, CustomerOrder = &amp;Order) AS OrdersPlacementBalances
		ON CustomerOrdersBalanceAndTurnovers.ProductsAndServices = OrdersPlacementBalances.ProductsAndServices
			AND CustomerOrdersBalanceAndTurnovers.Characteristic = OrdersPlacementBalances.Characteristic
			AND CustomerOrdersBalanceAndTurnovers.CustomerOrder = OrdersPlacementBalances.CustomerOrder
			AND CustomerOrdersBalanceAndTurnovers.Company = OrdersPlacementBalances.Company
		LEFT JOIN AccumulationRegister.Inventory.Balance(, CustomerOrder = VALUE(Document.CustomerOrder.EmptyRef)) AS AvailableBallance
		ON CustomerOrdersBalanceAndTurnovers.ProductsAndServices = AvailableBallance.ProductsAndServices
			AND CustomerOrdersBalanceAndTurnovers.Characteristic = AvailableBallance.Characteristic
				AND CustomerOrdersBalanceAndTurnovers.Company = AvailableBallance.Company</query>
