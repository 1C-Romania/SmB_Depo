			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	1 AS TypeRegister,
	CASE
		WHEN AccountsReceivableBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE AccountsReceivableBalancesAndTurnovers.Recorder
	END AS Recorder,
	AccountsReceivableBalancesAndTurnovers.SecondPeriod,
	AccountsReceivableBalancesAndTurnovers.MinutePeriod,
	AccountsReceivableBalancesAndTurnovers.HourPeriod,
	AccountsReceivableBalancesAndTurnovers.DayPeriod,
	AccountsReceivableBalancesAndTurnovers.WeekPeriod,
	AccountsReceivableBalancesAndTurnovers.TenDaysPeriod,
	AccountsReceivableBalancesAndTurnovers.MonthPeriod,
	AccountsReceivableBalancesAndTurnovers.QuarterPeriod,
	AccountsReceivableBalancesAndTurnovers.HalfYearPeriod,
	AccountsReceivableBalancesAndTurnovers.YearPeriod,
	AccountsReceivableBalancesAndTurnovers.Company,
	AccountsReceivableBalancesAndTurnovers.SettlementsType,
	AccountsReceivableBalancesAndTurnovers.Counterparty,
	AccountsReceivableBalancesAndTurnovers.Contract,
	AccountsReceivableBalancesAndTurnovers.Document,
	AccountsReceivableBalancesAndTurnovers.Order,
	AccountsReceivableBalancesAndTurnovers.AmountOpeningBalance,
	AccountsReceivableBalancesAndTurnovers.AmountClosingBalance,
	AccountsReceivableBalancesAndTurnovers.AmountTurnover,
	AccountsReceivableBalancesAndTurnovers.AmountReceipt,
	AccountsReceivableBalancesAndTurnovers.AmountExpense,
	AccountsReceivableBalancesAndTurnovers.AmountCurOpeningBalance AS CounterpartyOpnDebtCur,
	AccountsReceivableBalancesAndTurnovers.AmountCurClosingBalance AS CounterpartyClsDebtCur,
	AccountsReceivableBalancesAndTurnovers.AmountCurTurnover,
	AccountsReceivableBalancesAndTurnovers.AmountCurReceipt AS InCounterpartyDebtCur,
	AccountsReceivableBalancesAndTurnovers.AmountCurExpense AS DeCounterpartyDebtCur
	FROM
		AccumulationRegister.AccountsReceivable.BalanceAndTurnovers(, , Auto, , ) AS AccountsReceivableBalancesAndTurnovers</query>
