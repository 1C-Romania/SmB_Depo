		<dataSource>DataSource1</dataSource>
		<query>SELECT
	CounterpartyProductsAndServicesPrices.ProductsAndServices AS ProductsAndServices,
	CounterpartyProductsAndServicesPrices.Characteristic AS Characteristic,
	MAX(CounterpartyProductsAndServicesPrices.Period) AS Period
INTO MaximumPeriod
FROM
	InformationRegister.CounterpartyProductsAndServicesPrices AS CounterpartyProductsAndServicesPrices
WHERE
	CounterpartyProductsAndServicesPrices.Actuality
	AND (&amp;Period = DATETIME(1, 1, 1, 0, 0, 0)
			OR CounterpartyProductsAndServicesPrices.Period &lt;= &amp;Period)

GROUP BY
	CounterpartyProductsAndServicesPrices.Characteristic,
	CounterpartyProductsAndServicesPrices.ProductsAndServices

INDEX BY
	Period,
	Characteristic,
	ProductsAndServices
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	MaximumPeriod.ProductsAndServices AS ProductsAndServices,
	MaximumPeriod.Characteristic AS Characteristic,
	MAX(CounterpartyProductsAndServicesPrices.CounterpartyPriceKind.Owner) AS Vendor
INTO Vendors
FROM
	MaximumPeriod AS MaximumPeriod
		LEFT JOIN InformationRegister.CounterpartyProductsAndServicesPrices AS CounterpartyProductsAndServicesPrices
		ON MaximumPeriod.Period = CounterpartyProductsAndServicesPrices.Period
			AND MaximumPeriod.ProductsAndServices = CounterpartyProductsAndServicesPrices.ProductsAndServices
			AND MaximumPeriod.Characteristic = CounterpartyProductsAndServicesPrices.Characteristic
			AND (CounterpartyProductsAndServicesPrices.Actuality)

GROUP BY
	MaximumPeriod.Characteristic,
	MaximumPeriod.ProductsAndServices

INDEX BY
	ProductsAndServices,
	Characteristic
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	InventoryBalanceAndTurnovers.ProductsAndServices AS ProductsAndServices,
	InventoryBalanceAndTurnovers.Characteristic AS Characteristic,
	Vendors.Vendor AS Vendor
FROM
	AccumulationRegister.Inventory.BalanceAndTurnovers(, , Auto, , GLAccount.TypeOfAccount = VALUE(Enum.GLAccountsTypes.Inventory)) AS InventoryBalanceAndTurnovers
		LEFT JOIN Vendors AS Vendors
		ON InventoryBalanceAndTurnovers.ProductsAndServices = Vendors.ProductsAndServices
			AND InventoryBalanceAndTurnovers.Characteristic = Vendors.Characteristic</query>
