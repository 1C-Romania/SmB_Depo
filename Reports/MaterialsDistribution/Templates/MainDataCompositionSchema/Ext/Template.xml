	Inventory.StructuralUnitCorr AS ProductionSubdivision,
	Inventory.ProductsAndServicesCorr AS Products,
	Inventory.CharacteristicCorr AS ProductCharacteristic,
	Inventory.BatchCorr AS BatchProducts,
	Inventory.CorrGLAccount AS ProductsGLAccount,
	Inventory.CustomerCorrOrder AS CustomerOrderProducts,
	Inventory.SpecificationCorr AS ProductionSpecification,
	Inventory.ProductsAndServicesCorr.MeasurementUnit AS UnOfMeasProducts,
	0 AS ProductsQuantity,
	Inventory.ProductsAndServices AS ProductsAndServices,
	Inventory.Characteristic AS Characteristic,
	Inventory.Batch AS Batch,
	Inventory.StructuralUnit AS Division,
	Inventory.CustomerOrder AS CustomerOrder,
	Inventory.GLAccount AS GLAccount,
	Inventory.Specification AS Specification,
	Inventory.ProductsAndServices.MeasurementUnit AS UnOfMeas,
	Inventory.Quantity AS Quantity,
	Inventory.Recorder AS Recorder
