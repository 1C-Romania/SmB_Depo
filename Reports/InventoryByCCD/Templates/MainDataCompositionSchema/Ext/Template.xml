		<query>SELECT ALLOWED
	CASE
		WHEN InventoryByCCDBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryByCCDBalancesAndTurnovers.Recorder
	END AS Recorder,
	InventoryByCCDBalancesAndTurnovers.SecondPeriod,
	InventoryByCCDBalancesAndTurnovers.MinutePeriod,
	InventoryByCCDBalancesAndTurnovers.HourPeriod,
	InventoryByCCDBalancesAndTurnovers.DayPeriod,
	InventoryByCCDBalancesAndTurnovers.WeekPeriod,
	InventoryByCCDBalancesAndTurnovers.TenDaysPeriod,
	InventoryByCCDBalancesAndTurnovers.MonthPeriod,
	InventoryByCCDBalancesAndTurnovers.QuarterPeriod,
	InventoryByCCDBalancesAndTurnovers.HalfYearPeriod,
	InventoryByCCDBalancesAndTurnovers.YearPeriod,
	InventoryByCCDBalancesAndTurnovers.Company,
	InventoryByCCDBalancesAndTurnovers.ProductsAndServices,
	InventoryByCCDBalancesAndTurnovers.Characteristic,
	InventoryByCCDBalancesAndTurnovers.Batch,
	InventoryByCCDBalancesAndTurnovers.CCDNo,
	InventoryByCCDBalancesAndTurnovers.CountryOfOrigin,
	InventoryByCCDBalancesAndTurnovers.QuantityOpeningBalance,
	InventoryByCCDBalancesAndTurnovers.QuantityClosingBalance,
	InventoryByCCDBalancesAndTurnovers.QuantityTurnover,
	InventoryByCCDBalancesAndTurnovers.QuantityReceipt,
	InventoryByCCDBalancesAndTurnovers.QuantityExpense,
	InventoryByCCDBalancesAndTurnovers.ProductsAndServices.MeasurementUnit
	FROM
		AccumulationRegister.InventoryByCCD.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS InventoryByCCDBalancesAndTurnovers</query>
