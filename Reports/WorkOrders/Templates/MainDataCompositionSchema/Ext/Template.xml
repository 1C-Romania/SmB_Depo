		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN WorkOrdersTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE WorkOrdersTurnovers.Recorder
	END AS Recorder,
	WorkOrdersTurnovers.SecondPeriod,
	WorkOrdersTurnovers.MinutePeriod,
	WorkOrdersTurnovers.HourPeriod,
	WorkOrdersTurnovers.DayPeriod,
	WorkOrdersTurnovers.WeekPeriod,
	WorkOrdersTurnovers.TenDaysPeriod,
	WorkOrdersTurnovers.MonthPeriod,
	WorkOrdersTurnovers.QuarterPeriod,
	WorkOrdersTurnovers.HalfYearPeriod,
	WorkOrdersTurnovers.YearPeriod,
	WorkOrdersTurnovers.Counterparty,
	WorkOrdersTurnovers.Contract,
	WorkOrdersTurnovers.WorkKind AS WorkKind,
	WorkOrdersTurnovers.Employee AS Employee,
	WorkOrdersTurnovers.ProductsAndServices,
	WorkOrdersTurnovers.Characteristic,
	WorkOrdersTurnovers.CustomerOrder,
	WorkOrdersTurnovers.ImportPlanTurnover AS ImportPlan,
	WorkOrdersTurnovers.ImportActualTurnover AS ImportActual,
	ISNULL(WorkOrdersTurnovers.ImportActualTurnover, 0) - ISNULL(WorkOrdersTurnovers.ImportPlanTurnover, 0) AS ImportRejection,
	0 AS ImportingPercent,
	WorkOrdersTurnovers.AmountPlanTurnover AS AmountPlan,
	WorkOrdersTurnovers.AmountFactTurnover AS AmountFact,
	ISNULL(WorkOrdersTurnovers.AmountFactTurnover, 0) - ISNULL(WorkOrdersTurnovers.AmountPlanTurnover, 0) AS AmountRejection,
	0 AS AmountPercent
	FROM
		AccumulationRegister.WorkOrders.Turnovers(, , Auto, ) AS WorkOrdersTurnovers</query>
	</dataSet>
	<totalField>
		<dataPath>ImportRejection</dataPath>
		<expression>SUM(ImportRejection)</expression>
	</totalField>
	<totalField>
		<dataPath>ImportPlan</dataPath>
		<expression>SUM(ImportPlan)</expression>
	</totalField>
	<totalField>
		<dataPath>ImportingPercent</dataPath>
		<expression>CASE 
WHEN ISNULL(SUM(ImportPlan), 0) = 0 
THEN "-" 
ELSE ISNULL(SUM(ImportActual),0) / SUM(ImportPlan) * 100 
END</expression>
	</totalField>
	<totalField>
		<dataPath>ImportActual</dataPath>
		<expression>SUM(ImportActual)</expression>
	</totalField>
	<totalField>
		<dataPath>AmountPlan</dataPath>
		<expression>SUM(AmountPlan)</expression>
	</totalField>
	<totalField>
		<dataPath>AmountFact</dataPath>
		<expression>SUM(AmountFact)</expression>
	</totalField>
	<totalField>
		<dataPath>AmountRejection</dataPath>
		<expression>SUM(AmountRejection)</expression>
	</totalField>
	<totalField>
		<dataPath>AmountPercent</dataPath>
		<expression>CASE 
WHEN ISNULL(SUM(AmountPlan), 0) = 0 
THEN "-" 
ELSE ISNULL(SUM(AmountFact),0) / SUM(AmountPlan) * 100 
END</expression>
	</totalField>
