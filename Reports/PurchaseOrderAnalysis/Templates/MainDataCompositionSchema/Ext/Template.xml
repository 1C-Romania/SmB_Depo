			<dataPath>EntersWarehouse</dataPath>
			<field>EntersWarehouse</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Ordered</dataPath>
			<field>Ordered</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Received</dataPath>
			<field>Received</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	PurchaseOrdersBalanceAndTurnovers.ProductsAndServices AS ProductsAndServices,
	CASE
		WHEN PurchaseOrdersBalanceAndTurnovers.Characteristic = VALUE(Catalog.ProductsAndServicesCharacteristics.EmptyRef)
			THEN NULL
		ELSE PurchaseOrdersBalanceAndTurnovers.Characteristic
	END AS Characteristic,
	PurchaseOrdersBalanceAndTurnovers.QuantityReceipt AS Ordered,
	PurchaseOrdersBalanceAndTurnovers.QuantityExpense AS Received,
	PurchaseOrdersBalanceAndTurnovers.QuantityClosingBalance AS LeftToReceive,
	ISNULL(OrdersPlacementBalances.QuantityBalance, 0) AS EntersOrderReserve,
	CASE
		WHEN PurchaseOrdersBalanceAndTurnovers.ProductsAndServices.ProductsAndServicesType = VALUE(Enum.ProductsAndServicesTypes.InventoryItem)
			THEN PurchaseOrdersBalanceAndTurnovers.QuantityClosingBalance - ISNULL(OrdersPlacementBalances.QuantityBalance, 0)
		ELSE 0
	END AS EntersWarehouse
	FROM
	AccumulationRegister.PurchaseOrders.BalanceAndTurnovers(, , , , PurchaseOrder = &amp;Order) AS PurchaseOrdersBalanceAndTurnovers
		LEFT JOIN AccumulationRegister.OrdersPlacement.Balance(, SupplySource = &amp;Order) AS OrdersPlacementBalances
		ON PurchaseOrdersBalanceAndTurnovers.ProductsAndServices = OrdersPlacementBalances.ProductsAndServices
			AND PurchaseOrdersBalanceAndTurnovers.Characteristic = OrdersPlacementBalances.Characteristic
			AND PurchaseOrdersBalanceAndTurnovers.PurchaseOrder = OrdersPlacementBalances.SupplySource
				AND PurchaseOrdersBalanceAndTurnovers.Company = OrdersPlacementBalances.Company</query>
