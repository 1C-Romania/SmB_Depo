		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN InventoryInWarehousesBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryInWarehousesBalanceAndTurnovers.Recorder
	END AS Recorder,
	InventoryInWarehousesBalanceAndTurnovers.SecondPeriod,
	InventoryInWarehousesBalanceAndTurnovers.MinutePeriod,
	InventoryInWarehousesBalanceAndTurnovers.HourPeriod,
	InventoryInWarehousesBalanceAndTurnovers.DayPeriod,
	InventoryInWarehousesBalanceAndTurnovers.WeekPeriod,
	InventoryInWarehousesBalanceAndTurnovers.TenDaysPeriod,
	InventoryInWarehousesBalanceAndTurnovers.MonthPeriod,
	InventoryInWarehousesBalanceAndTurnovers.QuarterPeriod,
	InventoryInWarehousesBalanceAndTurnovers.HalfYearPeriod,
	InventoryInWarehousesBalanceAndTurnovers.YearPeriod,
	InventoryInWarehousesBalanceAndTurnovers.Company,
	InventoryInWarehousesBalanceAndTurnovers.ProductsAndServices,
	InventoryInWarehousesBalanceAndTurnovers.Characteristic,
	InventoryInWarehousesBalanceAndTurnovers.Batch,
	InventoryInWarehousesBalanceAndTurnovers.StructuralUnit,
	InventoryInWarehousesBalanceAndTurnovers.Cell,
	InventoryInWarehousesBalanceAndTurnovers.QuantityOpeningBalance,
	InventoryInWarehousesBalanceAndTurnovers.QuantityClosingBalance,
	InventoryInWarehousesBalanceAndTurnovers.QuantityTurnover,
	InventoryInWarehousesBalanceAndTurnovers.QuantityReceipt,
	InventoryInWarehousesBalanceAndTurnovers.QuantityExpense,
	InventoryInWarehousesBalanceAndTurnovers.ProductsAndServices.MeasurementUnit,
	NULL AS CountForEntryOpeningBalance,
	NULL AS CountForAdmissionClosingBalance,
	NULL AS TurnoverCount,
	NULL AS CountForEntryReceipt,
	NULL AS CountForEntryExpense,
	NULL AS CountOpeningBalanceFlow,
	NULL AS CountToClosingBalanceConsumption,
	NULL AS CountToConsumptionTurnover,
	NULL AS CountExpenseReceipt,
	NULL AS QuantityToExpenseExpense,
	"Total in all warehouses" AS TotalByWarehouses
	FROM
	AccumulationRegister.InventoryInWarehouses.BalanceAndTurnovers(, , Auto, , ) AS InventoryInWarehousesBalanceAndTurnovers

	UNION ALL

	SELECT
	InventoryForWarehousesBalanceAndTurnovers.Recorder,
	InventoryForWarehousesBalanceAndTurnovers.SecondPeriod,
	InventoryForWarehousesBalanceAndTurnovers.MinutePeriod,
	InventoryForWarehousesBalanceAndTurnovers.HourPeriod,
	InventoryForWarehousesBalanceAndTurnovers.DayPeriod,
	InventoryForWarehousesBalanceAndTurnovers.WeekPeriod,
	InventoryForWarehousesBalanceAndTurnovers.TenDaysPeriod,
	InventoryForWarehousesBalanceAndTurnovers.MonthPeriod,
	InventoryForWarehousesBalanceAndTurnovers.QuarterPeriod,
	InventoryForWarehousesBalanceAndTurnovers.HalfYearPeriod,
	InventoryForWarehousesBalanceAndTurnovers.YearPeriod,
	InventoryForWarehousesBalanceAndTurnovers.Company,
	InventoryForWarehousesBalanceAndTurnovers.ProductsAndServices,
	InventoryForWarehousesBalanceAndTurnovers.Characteristic,
	InventoryForWarehousesBalanceAndTurnovers.Batch,
	InventoryForWarehousesBalanceAndTurnovers.StructuralUnit,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	InventoryForWarehousesBalanceAndTurnovers.ProductsAndServices.MeasurementUnit,
	InventoryForWarehousesBalanceAndTurnovers.QuantityOpeningBalance,
	InventoryForWarehousesBalanceAndTurnovers.QuantityClosingBalance,
	InventoryForWarehousesBalanceAndTurnovers.QuantityTurnover,
	InventoryForWarehousesBalanceAndTurnovers.QuantityReceipt,
	InventoryForWarehousesBalanceAndTurnovers.QuantityExpense,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	"Total in all warehouses"
	FROM
	AccumulationRegister.InventoryForWarehouses.BalanceAndTurnovers(, , Auto, , ) AS InventoryForWarehousesBalanceAndTurnovers

	UNION ALL

	SELECT
	InventoryFromWarehousesBalanceAndTurnovers.Recorder,
	InventoryFromWarehousesBalanceAndTurnovers.SecondPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.MinutePeriod,
	InventoryFromWarehousesBalanceAndTurnovers.HourPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.DayPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.WeekPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.TenDaysPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.MonthPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.QuarterPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.HalfYearPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.YearPeriod,
	InventoryFromWarehousesBalanceAndTurnovers.Company,
	InventoryFromWarehousesBalanceAndTurnovers.ProductsAndServices,
	InventoryFromWarehousesBalanceAndTurnovers.Characteristic,
	InventoryFromWarehousesBalanceAndTurnovers.Batch,
	InventoryFromWarehousesBalanceAndTurnovers.StructuralUnit,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	InventoryFromWarehousesBalanceAndTurnovers.ProductsAndServices.MeasurementUnit,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	InventoryFromWarehousesBalanceAndTurnovers.QuantityOpeningBalance,
	InventoryFromWarehousesBalanceAndTurnovers.QuantityClosingBalance,
	InventoryFromWarehousesBalanceAndTurnovers.QuantityTurnover,
	InventoryFromWarehousesBalanceAndTurnovers.QuantityReceipt,
	InventoryFromWarehousesBalanceAndTurnovers.QuantityExpense,
	"Total in all warehouses"
	FROM
		AccumulationRegister.InventoryFromWarehouses.BalanceAndTurnovers(, , Auto, , ) AS InventoryFromWarehousesBalanceAndTurnovers</query>
