				<QueryText>SELECT
	InformationRegisterDataAreas.DataArea,
	InformationRegisterDataAreas.Status,
	InformationRegisterDataAreas.Repeat,
	InformationRegisterDataAreas.ExportID,
	InformationRegisterDataAreas.ProcessingError,
	InformationRegisterDataAreas.Variant,
	DataAreaPresentation.Value AS Presentation,
	DataAreaTimeZone.Value AS TimeZone,
	DataAreasPrefix.Value AS Prefix
