				<QueryText>SELECT
	DocumentCustomerInvoiceNote.Ref,
	DocumentCustomerInvoiceNote.DeletionMark,
	DocumentCustomerInvoiceNote.Number,
	DocumentCustomerInvoiceNote.Date,
	DocumentCustomerInvoiceNote.Posted,
	DocumentCustomerInvoiceNote.Company,
	DocumentCustomerInvoiceNote.OperationKind,
	DocumentCustomerInvoiceNote.Counterparty,
	DocumentCustomerInvoiceNote.Contract,
	DocumentCustomerInvoiceNote.DocumentCurrency,
	DocumentCustomerInvoiceNote.Multiplicity,
	DocumentCustomerInvoiceNote.ExchangeRate,
	DocumentCustomerInvoiceNote.Comment,
	DocumentCustomerInvoiceNote.BasisDocument,
	DocumentCustomerInvoiceNote.IncomingDocumentNumber,
	DocumentCustomerInvoiceNote.IncomingDocumentDate,
	DocumentCustomerInvoiceNote.DocumentAmount,
	DocumentCustomerInvoiceNote.Author,
	DocumentCustomerInvoiceNote.Consignor,
	DocumentCustomerInvoiceNote.Consignee,
	DocumentCustomerInvoiceNote.Same,
	DocumentCustomerInvoiceNote.Correction,
	DocumentCustomerInvoiceNote.CorrectionCustomerInvoiceNote,
	DocumentCustomerInvoiceNote.CorrectionNumber,
	DocumentCustomerInvoiceNote.InitialDocumentNumber,
	DocumentCustomerInvoiceNote.InitialDocumentDate,
	DocumentCustomerInvoiceNote.DateOfExtension,
	DocumentCustomerInvoiceNote.StampBase,
	DocumentCustomerInvoiceNote.Inventory.(
		Ref,
		LineNumber,
		ProductsAndServices,
		Content,
		Characteristic,
		Batch,
		Quantity,
		MeasurementUnit,
		Price,
		Amount,
		VATRate,
		VATAmount,
		Total,
		CountryOfOrigin,
		CCDNo
	),
	DocumentCustomerInvoiceNote.BasisDocuments.(
		Ref,
		LineNumber,
		BasisDocument
	),
	DocumentCustomerInvoiceNote.PaymentDocumentsDateNumber.(
		Ref,
		LineNumber,
		PaymentAccountingDocumentDate,
		PaymentAccountingDocumentNumber
	),
	DocumentCustomerInvoiceNote.PointInTime,
	CASE
		WHEN CAST(DocumentCustomerInvoiceNote.BasisDocument AS Document.CustomerInvoice).OperationKind IN (VALUE(Enum.OperationKindsCustomerInvoice.ReturnToVendor), VALUE(Enum.OperationKindsCustomerInvoice.ReturnToPrincipal), VALUE(Enum.OperationKindsCustomerInvoice.ReturnFromProcessing), VALUE(Enum.OperationKindsCustomerInvoice.ReturnFromSafeCustody))
			THEN TRUE
		ELSE FALSE
	END AS ForReturn
	FROM
		Document.CustomerInvoiceNote AS DocumentCustomerInvoiceNote</QueryText>
