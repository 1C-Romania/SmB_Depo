	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CASE
		WHEN DocumentCashOutflowPlan.Posted
			THEN CASE
					WHEN (DocumentCashOutflowPlan.PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.NotApproved)
							OR ISNULL(PaymentEvent.DocumentAmount, 0) &lt;&gt; 0)
							AND ISNULL(PaymentEvent.DocumentAmount, 0) - ISNULL(PaymentEvent.AmountOfPlanBalance, 0) = 0
						THEN CASE
								WHEN DocumentCashOutflowPlan.PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
										AND Not PaymentSchedule.InvoiceForPayment IS NULL 
									THEN CASE
											WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
												THEN 6
											WHEN PaymentSchedule.Period = &amp;CurrentDateSession
												THEN 4
											ELSE 2
										END
								ELSE 2
							END
					WHEN ISNULL(PaymentEvent.DocumentAmount, 0) &lt;= ISNULL(PaymentEvent.DocumentAmount, 0) - ISNULL(PaymentEvent.AmountOfPlanBalance, 0)
						THEN 0
					ELSE CASE
							WHEN DocumentCashOutflowPlan.PaymentConfirmationStatus = VALUE(Enum.PaymentApprovalStatuses.Approved)
									AND Not PaymentSchedule.InvoiceForPayment IS NULL 
								THEN CASE
										WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
											THEN 5
										WHEN PaymentSchedule.Period = &amp;CurrentDateSession
											THEN 3
										ELSE 1
									END
							ELSE 1
						END
				END
		ELSE 2
	END AS PaymentPictureNumber,
	CASE
		WHEN DocumentCashOutflowPlan.Posted
				AND DocumentCashOutflowPlan.DocumentAmount &gt; 0
			THEN CAST((ISNULL(PaymentEvent.DocumentAmount, 0) - ISNULL(PaymentEvent.AmountOfPlanBalance, 0)) * 100 / DocumentCashOutflowPlan.DocumentAmount AS NUMBER(15, 2))
		ELSE 0
	END AS PaymentPercentage,
	ISNULL(PaymentEvent.DocumentAmount, 0) - ISNULL(PaymentEvent.AmountOfPlanBalance, 0) AS AmountPaid,
	DocumentCashOutflowPlan.Ref,
	DocumentCashOutflowPlan.DeletionMark,
	DocumentCashOutflowPlan.Number,
	DocumentCashOutflowPlan.Date,
	DocumentCashOutflowPlan.Posted,
	DocumentCashOutflowPlan.Company,
	DocumentCashOutflowPlan.Comment,
	DocumentCashOutflowPlan.DocumentAmount,
	DocumentCashOutflowPlan.DocumentCurrency,
	DocumentCashOutflowPlan.CashFlowItem,
	DocumentCashOutflowPlan.CashAssetsType,
	DocumentCashOutflowPlan.BankAccount,
	DocumentCashOutflowPlan.PettyCash,
	DocumentCashOutflowPlan.BasisDocument,
	DocumentCashOutflowPlan.PaymentConfirmationStatus,
	DocumentCashOutflowPlan.IncomingDocumentNumber,
	DocumentCashOutflowPlan.IncomingDocumentDate,
	DocumentCashOutflowPlan.Counterparty,
	DocumentCashOutflowPlan.Contract,
	DocumentCashOutflowPlan.Author,
	DocumentCashOutflowPlan.PointInTime
	FROM
	Document.CashOutflowPlan AS DocumentCashOutflowPlan
		{LEFT JOIN InformationRegister.OrdersPaymentSchedule AS PaymentSchedule
		ON DocumentCashOutflowPlan.Ref = PaymentSchedule.InvoiceForPayment
			AND (PaymentSchedule.Period &lt;= &amp;CurrentDateSession)}
		LEFT JOIN InformationRegister.PaymentsSchedule AS PaymentEvent
			ON DocumentCashOutflowPlan.Ref = PaymentEvent.InvoiceForPayment</QueryText>
				<MainTable>Document.CashOutflowPlan</MainTable>
