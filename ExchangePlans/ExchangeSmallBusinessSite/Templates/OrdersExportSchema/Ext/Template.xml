		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	Orders.Ref AS CustomerOrder,
	CustomerOrdersFromSite.OrderNumberOnSite AS OrderNumberOnSite,
	CustomerOrdersFromSite.OrderDateOnSite AS OrderDateOnSite,
	Orders.DocumentCurrency AS Currency,
	Orders.DocumentAmount AS DocumentAmount
INTO TemporaryTableOrders
FROM
	InformationRegister.CustomerOrdersFromSite AS CustomerOrdersFromSite
		INNER JOIN Document.CustomerOrder AS Orders
		ON CustomerOrdersFromSite.CustomerOrder = Orders.Ref
WHERE
	(NOT Orders.Closed
			OR Orders.Closed
				AND Orders.OrderState = &amp;OrderStatusInProcess)
	AND Orders.OperationKind = VALUE(Enum.OperationKindsCustomerOrder.OrderForSale)
{WHERE
	Orders.Ref.* AS CustomerOrder}
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	CustomerOrdersBalances.CustomerOrder AS CustomerOrder,
	CustomerOrdersBalances.QuantityBalance AS QuantityBalance
INTO TemporaryTableShipmentBalance
FROM
	AccumulationRegister.CustomerOrders.Balance(
			,
			CustomerOrder In
				(SELECT
					TemporaryTableOrders.CustomerOrder
				FROM
					TemporaryTableOrders AS TemporaryTableOrders)) AS CustomerOrdersBalances
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TemporaryTableOrders.CustomerOrder AS CustomerOrder,
	MAX(CustomerOrders.Recorder) AS ShipmentDocument
INTO TemporaryTableShipmentDocuments
FROM
	TemporaryTableOrders AS TemporaryTableOrders
		INNER JOIN AccumulationRegister.CustomerOrders AS CustomerOrders
		ON TemporaryTableOrders.CustomerOrder = CustomerOrders.CustomerOrder
			AND (CustomerOrders.RecordType = VALUE(AccumulationRecordType.Expense))

GROUP BY
	TemporaryTableOrders.CustomerOrder
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	InvoicesAndOrdersPaymentTurnovers.InvoiceForPayment AS CustomerOrder,
	InvoicesAndOrdersPaymentTurnovers.AmountTurnover - InvoicesAndOrdersPaymentTurnovers.PaymentAmountTurnover - InvoicesAndOrdersPaymentTurnovers.AdvanceAmountTurnover AS LeftToPay
INTO TemporaryTableLeftToPay
FROM
	AccumulationRegister.InvoicesAndOrdersPayment.Turnovers(
			,
			,
			,
			InvoiceForPayment In
				(SELECT
					TemporaryTableOrders.CustomerOrder
				FROM
					TemporaryTableOrders AS TemporaryTableOrders)) AS InvoicesAndOrdersPaymentTurnovers
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TemporaryTableOrders.CustomerOrder AS CustomerOrder,
	MAX(AccountsReceivable.Recorder) AS PaymentDocument,
	SUM(AccountsReceivable.AmountCur) AS PaymentAmount
INTO TemporaryTablePaymentDocuments
FROM
	TemporaryTableOrders AS TemporaryTableOrders
		INNER JOIN AccumulationRegister.AccountsReceivable AS AccountsReceivable
		ON TemporaryTableOrders.CustomerOrder = AccountsReceivable.Order
			AND (AccountsReceivable.RecordType = VALUE(AccumulationRecordType.Expense))

GROUP BY
	TemporaryTableOrders.CustomerOrder
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	TemporaryTableOrders.CustomerOrder AS CustomerOrder,
	MAX(CustomerOrderPaymentCalendar.PayDate) AS PaymentDate
INTO TemporaryTablePaySchedule
FROM
	TemporaryTableOrders AS TemporaryTableOrders
		INNER JOIN Document.CustomerOrder.PaymentCalendar AS CustomerOrderPaymentCalendar
		ON TemporaryTableOrders.CustomerOrder = CustomerOrderPaymentCalendar.Ref

GROUP BY
	TemporaryTableOrders.CustomerOrder
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TemporaryTableOrders.CustomerOrder AS CustomerOrder,
	TemporaryTableOrders.OrderNumberOnSite AS OrderNumberOnSite,
	TemporaryTableOrders.OrderDateOnSite AS OrderDateOnSite,
	ISNULL(TemporaryTableLeftToPay.LeftToPay, 0) AS BalanceForPayment,
	ISNULL(TemporaryTableShipmentBalance.QuantityBalance, 0) AS ResidueForShipment,
	TemporaryTableOrders.CustomerOrder.Date AS Date,
	TemporaryTableOrders.CustomerOrder.Number AS Number,
	TemporaryTableOrders.CustomerOrder.Posted AS Posted,
	TemporaryTableOrders.CustomerOrder.DeletionMark AS DeletionMark,
	ISNULL(TemporaryTablePaySchedule.PaymentDate, DATETIME(1, 1, 1, 0, 0, 0)) AS PaymentDate,
	ISNULL(TemporaryTablePaymentDocuments.PaymentDocument.Number, "") AS PaymentNo,
	ISNULL(TemporaryTablePaymentDocuments.PaymentDocument.Date, DATETIME(1, 1, 1, 0, 0, 0)) AS PayDate,
	ISNULL(TemporaryTableShipmentDocuments.ShipmentDocument.Number, "") AS ShipmentNo,
	ISNULL(TemporaryTableShipmentDocuments.ShipmentDocument.Date, DATETIME(1, 1, 1, 0, 0, 0)) AS ShipmentDate,
	TemporaryTableOrders.Currency AS Currency,
	TemporaryTableOrders.DocumentAmount AS DocumentAmount,
	TemporaryTableOrders.CustomerOrder.VATTaxation AS VATTaxation,
	TemporaryTableOrders.CustomerOrder.AmountIncludesVAT AS AmountIncludesVAT,
	TemporaryTableOrders.CustomerOrder.OrderState AS OrderState,
	TemporaryTableOrders.CustomerOrder.OrderState.OrderStatus AS OrderStatus,
	TemporaryTableOrders.CustomerOrder.Closed AS Closed,
	TemporaryTableOrders.CustomerOrder.Comment AS Comment,
	TemporaryTableOrders.CustomerOrder.BankAccount AS BankAccount,
	TemporaryTableOrders.CustomerOrder.Counterparty AS Counterparty,
	TemporaryTableOrders.CustomerOrder.Counterparty.Description AS Description,
	TemporaryTableOrders.CustomerOrder.Counterparty.FullDescr AS DescriptionFull,
	TemporaryTableOrders.CustomerOrder.Counterparty.LegalEntityIndividual AS LegalEntityIndividual,
	TemporaryTableOrders.CustomerOrder.Counterparty.TIN AS TIN,
	TemporaryTableOrders.CustomerOrder.Counterparty.KPP AS KPP,
	TemporaryTableOrders.CustomerOrder.Counterparty.CodeByOKPO AS CodeByOKPO,
	ISNULL(CurrencyRatesSliceLast.ExchangeRate, 1) AS ExchangeRate,
	TemporaryTableOrders.CustomerOrder.Inventory.(
		ProductsAndServices AS ProductsAndServices,
		ProductsAndServices.ProductsAndServicesType AS ProductsAndServicesType,
		Characteristic AS Characteristic,
		Count AS Count,
		Price AS Price,
		DiscountMarkupPercent AS DiscountMarkupPercent,
		Amount AS Amount,
		VATRate AS VATRate,
		VATAmount AS VATAmount,
		TotalAmount AS TotalAmount,
		ShipmentDate AS ShipmentDate,
		ProductsAndServices.SKU AS SKU,
		ProductsAndServices.MeasurementUnit AS MeasurementUnit,
		ProductsAndServices.MeasurementUnit.Code AS MeasurementUnitCode,
		ProductsAndServices.MeasurementUnit.Description AS MeasurementUnitDescription,
		ProductsAndServices.MeasurementUnit.DescriptionFull AS MeasurementUnitDescriptionFull,
		ProductsAndServices.MeasurementUnit.InternationalAbbreviation AS MeasurementUnitInternationalAbbreviation,
		ProductsAndServices.VATRate AS ProductsAndServicesVATRate
	) AS Products
FROM
	TemporaryTableOrders AS TemporaryTableOrders
		LEFT JOIN TemporaryTablePaySchedule AS TemporaryTablePaySchedule
		ON TemporaryTableOrders.CustomerOrder = TemporaryTablePaySchedule.CustomerOrder
		LEFT JOIN TemporaryTableShipmentDocuments AS TemporaryTableShipmentDocuments
		ON TemporaryTableOrders.CustomerOrder = TemporaryTableShipmentDocuments.CustomerOrder
		LEFT JOIN TemporaryTablePaymentDocuments AS TemporaryTablePaymentDocuments
		ON TemporaryTableOrders.CustomerOrder = TemporaryTablePaymentDocuments.CustomerOrder
		LEFT JOIN InformationRegister.CurrencyRates.SliceLast(, ) AS CurrencyRatesSliceLast
		ON TemporaryTableOrders.Currency = CurrencyRatesSliceLast.Currency
		LEFT JOIN TemporaryTableShipmentBalance AS TemporaryTableShipmentBalance
		ON TemporaryTableOrders.CustomerOrder = TemporaryTableShipmentBalance.CustomerOrder
		LEFT JOIN TemporaryTableLeftToPay AS TemporaryTableLeftToPay
		ON TemporaryTableOrders.CustomerOrder = TemporaryTableLeftToPay.CustomerOrder

ORDER BY
	CustomerOrder</query>
